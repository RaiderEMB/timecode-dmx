;----------------------------------------------------------------------------
; Macros

MACRO "Reset"         1   8
"data"                0  C0
"delay"               1  32
"data"                0  40
MEND

;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 1  ;Pan
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 3  ;Tilt
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 5  ;Colour
0   11   "Open"
12  23   "Blue"
24  35   "Deep Blue"
36  47   "Cyan"
48  59   "Magenta"
60  71   "Pink"
72  83   "Orange"
84  95   "Yellow"
96  107  "Light Blue"
108 119  "Deep Cyan"
120 131  "Green"
132 143  "Red"
144 189  "Sc CW %3d%%"100-((c-a)*100/(b-a))
190 190  "No Scroll"
191 255  "Sc CCW %3d%%"(c-a)*100/(b-a)
REND

RANGE 6  ;Shutter
0   63   "Closed"
64  127  "Rnd St %3d%%"(c-a)*100/(b-a)
128 191  "Strobe %3d%%"(c-a)*100/(b-a)
192 255  "Open"
REND

RANGE 7  ;Dimmer
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

RANGE 8  ;Control
0   127  "Safe"
128 255  "Reset"
REND

RANGE 9  ;PT Macros
0   41   "No Macro"
42  83   "Macro 1"
84  125  "Macro 2"
126 167  "Macro 3"
168 209  "Macro 4"
210 255  "Macro 5"
REND

RANGE 10  ;P/T Speed
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

