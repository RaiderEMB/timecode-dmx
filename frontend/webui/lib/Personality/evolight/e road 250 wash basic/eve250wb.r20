"Right"              23  B0  80
"Top"                24  80  4F
"Bottom"             25  80  B0
"TLeft"              26  4F  4F
"TRight"             27  B0  B0
"BLeft"              28  4F  B0
"BRight"             29  B0  4F
PFEND

PFTABLE "Colour"     -1   5
"Open"                1   6
"Red"                 2  8A
"Orange"              3  4E
"Yellow"              4  5A
"Green"               5  7E
"Cyan"                6  2A
"Blue"                7  12
