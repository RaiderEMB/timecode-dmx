Device ADJ_AWs250

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 -265.000000 265.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 -135.000000 135.000000
End

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 24 White
    Mapping 25 49 RGB240,0,0
    Mapping 50 74 RGB0,0,240
    Mapping 75 99 RGB0,240,0
    Mapping 100 124 RGB240,240,120
    Mapping 125 149 RGB240,0,240
    Mapping 150 174 RGB240,120,0
    Mapping 175 199 RGB120,0,240
End

// Colour
ChanRef Color Wheel 1
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 200 255 5 50
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 31 0 0
    Mapping 32 63 1 1
    Mapping 64 95 30 300
    Mapping 96 127 1 1
    Mapping 160 191 1 1
    Mapping 192 223 150 150
    Mapping 224 255 1 1
End

// Shutter
ChanRef Dimmer
    Vismode Pulse
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 128 159 10 100
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

