  <Macro>
    <Macro Name="Home">
      <Set Channel="Dimmer:1:0,Dimmer2:1:0,Shutter:2" Delay="1000" />
      <Set Channel="Control:5" Delay="5000" />
      <Set Channel="Control:1,Shutter:1,Dimmer:1:100,Dimmer2:1:100" Delay="0" />
