      <Function ID="3" Name="BO on Col" Display="'BO on Col %.f%%',0~100" Dmx="128~159" />
      <Function ID="4" Name="No Function" Display="'No Function'" Dmx="160~223" />
      <Function ID="5" Name="BO on P/T/Col" Display="'BO on P/T/Col %.f%%',0~100" Dmx="224~255" />
    </Attribute>
  </Control>
