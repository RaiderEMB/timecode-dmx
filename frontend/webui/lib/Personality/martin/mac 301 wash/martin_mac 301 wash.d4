    <Attribute ID="Colour1Func" Name="Colour Func" Description="" Group="C" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Fixed_Func" Display="'Fixed'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="2" Name="Split_Func" Display="'Split Gel'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="3" Name="Index_Func" Display="'Index'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="4" Name="Scroll_Func" Display="'Scroll_Func'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="5" Name="Raw_Func" Display="'Raw DMX'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Colour1" Name="Colour" Description="" Group="C">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed_Func" ID="1,2,4,6,8,10,12,14,16" If="'Colour1Func'==1" />
      <Condition Name="Split_Func" ID="3,5,7,9,11,13,15,17" If="'Colour1Func'==2" />
      <Condition Name="Scroll_Func" ID="19,20,21,22" If="'Colour1Func'==4" />
      <Condition Name="Index_Func" ID="18" If="'Colour1Func'==3" />
      <Else ID="23" />
      <Function ID="1" Name="RGB Mixing" Display="'RGB Mixing'" Dmx="0~1" Colour="0,0,0" />
      <Function ID="2" Name="Open" Display="'Open'" Dmx="2~3" Colour="255,255,255" />
      <Function ID="3" Name="Open/Red" Display="'Open/Red %.f%%',0~100" Dmx="4~7" Colour="255,255,255" />
      <Function ID="4" Name="Red" Display="'Red'" Dmx="8~11" Colour="255,0,0" />
      <Function ID="5" Name="Red/Yellow" Display="'Red/Yellow    %.f%%',0~100" Dmx="12~15" Colour="255,0,0" />
      <Function ID="6" Name="Yellow" Display="'Yellow'" Dmx="16~19" Colour="255,255,0" />
      <Function ID="7" Name="Yellow/Mag" Display="'Yellow/Mag %.f%%',0~100" Dmx="20~23" Colour="255,128,128" />
      <Function ID="8" Name="Magenta" Display="'Magenta'" Dmx="24~27" Colour="255,0,255" />
      <Function ID="9" Name="Magenta/Green" Display="'Magenta/Green %.f%%',0~100" Dmx="28~31" Colour="128,128,128" />
      <Function ID="10" Name="Green" Display="'Green'" Dmx="32~35" Colour="0,255,0" />
      <Function ID="11" Name="Green/Amber" Display="'Green/Amber   %.f%%',0~100" Dmx="36~39" Colour="128,178,0" />
      <Function ID="12" Name="Amber" Display="'Amber'" Dmx="40~43" Colour="255,100,0" />
      <Function ID="13" Name="Amber/Blue" Display="'Amber/Blue    %.f%%',0~100" Dmx="44~47" Colour="128,50,128" />
      <Function ID="14" Name="Blue" Display="'Blue'" Dmx="48~51" Colour="0,0,255" />
      <Function ID="15" Name="Blue/Cyan" Display="'Blue/Cyan %.f%%',0~100" Dmx="52~55" Colour="0,128,255" />
      <Function ID="16" Name="Cyan" Display="'Cyan'" Dmx="56~59" Colour="0,255,255" />
      <Function ID="17" Name="Cyan/White" Display="'Cyan/White    %.f%%',0~100" Dmx="60~63" Colour="128,255,255" />
      <Function ID="18" Name="Index" Display="'Index %.1f%%',0.0~100.0" Dmx="64~192" Colour="0,0,0" />
      <Function ID="19" Name="No Scroll" Display="'No Scroll'" Dmx="223~224" Colour="255,255,255" />
      <Function ID="20" Name="Scroll CW" Display="'Scroll CW     %.f%%',100~0" Dmx="193~222" Colour="0,0,0" />
      <Function ID="21" Name="No Scroll" Display="'No Scroll'" Dmx="223~224" Colour="255,255,255" />
      <Function ID="22" Name="Scroll CCW" Display="'Scroll CCW    %.f%%',0~100" Dmx="225~255" Colour="0,0,0" />
      <Function ID="23" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="PT_Speed" Name="P/T Speed" Description="" Group="S">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Tracking" Display="'Tracking'" Dmx="0~3" />
