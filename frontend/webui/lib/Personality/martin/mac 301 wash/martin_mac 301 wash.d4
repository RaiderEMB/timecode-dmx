      <Function ID="3" Name="8500K Fade" Display="'8500K Fade %.f%%',0~100" Dmx="8~15" />
      <Function ID="4" Name="6500K Fade" Display="'6500K Fade %.f%%',0~100" Dmx="16~23" />
      <Function ID="5" Name="White Bal Fade" Display="'White Bal Fade %.f%%',0~100" Dmx="24~31" />
