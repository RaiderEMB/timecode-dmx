    <Attribute ID="Shutter" Name="Shutter" Description="" Group="I">
      <Locate Locate="2" PowerOn="2" />
      <Function ID="1" Name="Closed" Display="'Closed'" Dmx="0~15" Intensity="0~0" />
      <Function ID="2" Name="Open" Display="'Open'" Dmx="16~95" Intensity="0~0" />
      <Function ID="3" Name="Strobe &gt; 10Hz" Display="'Strobe &gt; 10Hz %.f%%',0~100" Dmx="110~96" Intensity="0~0" />
      <Function ID="4" Name="Dimmer Fade" Display="'Dimmer Fade %.f%%',0~100" Dmx="112~125" Intensity="0~0" />
      <Function ID="5" Name="Strobe &lt; 10 Hz" Display="'Strobe &lt; 10 Hz %.f%%',0~100" Dmx="142~128" Intensity="0~0" />
      <Function ID="6" Name="Close Pulse" Display="'Close Pulse   %.f%%',0~100" Dmx="144~158" Intensity="0~0" />
      <Function ID="7" Name="Fade to 0" Display="'Fade to 0 %.f%%',0~100" Dmx="160~174" Intensity="0~0" />
      <Function ID="8" Name="Fade to 100" Display="'Fade to 100   %.f%%',0~100" Dmx="176~190" Intensity="0~0" />
      <Function ID="9" Name="Rnd Str 100%" Display="'Rnd Str 100 %.f%%',0~100" Dmx="192~206" Intensity="0~0" />
      <Function ID="10" Name="Rnd Str 0%" Display="'Rnd Str 0 %.f%%',0~100" Dmx="208~222" Intensity="0~0" />
      <Function ID="11" Name="Rnd Fade-&gt;0" Display="'Rnd Fade-&gt;0 %.f%%',0~100" Dmx="224~238" Intensity="0~0" />
      <Function ID="12" Name="Rnd Fade-&gt;100" Display="'Rnd Fade-&gt;100 %.f%%',0~100" Dmx="240~254" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Dimmer" Name="Dimmer" Description="" Group="I">
      <Locate Locate="1:100" PowerOn="1:0" />
      <Function ID="1" Name="Dimmer" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Zoom" Name="Zoom" Description="" Group="B">
