      <Function ID="16" Name="Red/Purple" Display="'Red/Purple'" Dmx="36~47" Colour="255,0,0" />
      <Function ID="17" Name="Red/Cyan" Display="'Red/Cyan'" Dmx="48~59" Colour="255,0,0" />
