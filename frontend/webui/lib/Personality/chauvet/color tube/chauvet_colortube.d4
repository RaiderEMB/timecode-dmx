      <Function ID="13" Name="Red/Green" Display="'Red/Green'" Dmx="0~11" Colour="255,0,0" />
      <Function ID="14" Name="Red/Yellow" Display="'Red/Yellow'" Dmx="12~23" Colour="255,0,0" />
