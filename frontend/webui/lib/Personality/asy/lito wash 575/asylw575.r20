MACRO "Lamp On"       1   14
"data"                0  32
"delay"               1  32
"data"                0   A
MEND

MACRO "Lamp Off"      2   14
"data"                0  46
"delay"               1  32
"data"                0   A
MEND

MACRO "Reset"         3   14
"data"                0  5A
"delay"               1  32
"data"                0   A
MEND

