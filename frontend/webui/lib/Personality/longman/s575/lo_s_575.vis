Device LO_S_575

// Shutter
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 255 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 50 0 0
    Mapping 51 101 1 1
    Mapping 102 152 30 300
    Mapping 153 203 1 1
    Mapping 204 255 1 1
End

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 7
    Mapping 0 255 -180.000000 180.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 8
    Mapping 0 255 -45.000000 45.000000
End

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 31 White
    Mapping 32 63 RGB240,0,0
    Mapping 64 95 RGB240,120,0
    Mapping 96 127 RGB240,240,200
    Mapping 128 159 RGB0,120,240
    Mapping 160 191 RGB0,240,0
    Mapping 192 223 RGB0,0,180
    Mapping 224 255 RGB240,240,0
End

// Gobo
ChanRef Gobo 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 50 Twelve Point Star
    Mapping 51 101 Number One
    Mapping 102 152 Number Two
    Mapping 153 203 Number Three
    Mapping 204 255 Number Four
End

// Gobo Rot
ChanRef Gobo 1 Rotate
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 127 0 360
End

// Gobo Rot
ChanRef Gobo 1 Rotate
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 128 191 -5 -20
    Mapping 192 255 20 5
End

