;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 1  ;Colour
0   26   "Open"
27  52   "Red"
53  78   "Orchid"
79  104  "Yellow"
105 130  "Green"
131 156  "Dark Orchid"
157 181  "Yellow"
182 199  "Rose"
200 255  "Scroll %3d%%"(c-a)*100/(b-a)
REND

RANGE 2  ;Gobo
0   26   "Open"
27  52   "Gobo 1"
53  78   "Gobo 2"
79  104  "Gobo 3"
105 130  "Gobo 4"
131 156  "Gobo 5"
157 182  "Gobo 6"
183 208  "Gobo 7"
209 255  "Scroll %3d%%"(c-a)*100/(b-a)
REND

RANGE 3  ;Gobo Rot
0   0    "No Spin"
1   85   "Sp CW %3d%%"(c-a)*100/(b-a)
86  170  "Sp CCW %3d%%"(c-a)*100/(b-a)
171 178  "No Spin"
179 255  "Sp Alt %3d%%"(c-a)*100/(b-a)
REND

RANGE 4  ;Shutter
0   3    "Closed"
4   47   "Open"
48  207  "Strobe %3d%%"(c-a)*100/(b-a)
208 253  "Open"
254 255  "Reset"
