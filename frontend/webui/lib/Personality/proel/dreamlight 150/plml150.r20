RANGE 1  ;Colour
0   7    "Open"
8   15   "Op/Mauve"
16  22   "Mauve"
23  31   "Mauve/Or"
38  47   "Orange"
55  63   "Or/Green"
64  70   "Green"
71  79   "Gn/Mag"
80  86   "Magenta"
87  95   "Mag/Yell"
96  100  "Yellow"
101 111  "Yell/Red"
112 120  "Red"
121 255  "Scroll %3d%%"0+(((c-a)*100)/(b-a))
REND

RANGE 2  ;Gobo Rot
0   127  "Index %3d%%"0+(((c-a)*100)/(b-a))
128 188  "Sp CW %3d%%"0+(((c-a)*100)/(b-a))
189 191  "No Spin"
192 255  "Sp CCW %3d%%"0+(((c-a)*100)/(b-a))
REND

RANGE 3  ;Gobo
