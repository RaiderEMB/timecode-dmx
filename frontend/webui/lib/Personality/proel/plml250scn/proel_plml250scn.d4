      <Function ID="3" Name="Raw_Func" Display="'Raw DMX'" Dmx="0~0" Update="Gobo1Rot, Gobo1" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Gobo1Rot" Name="Gobo Rot" Description="" Group="G">
      <Locate Locate="1:0" PowerOn="1:0" />
