      <Condition Name="Fixed_Func_Spin" ID="2,3,4,5" If="'Gobo1Func'==2" />
      <Condition Name="Raw_Func" ID="6" If="'Gobo1Func'==3" />
      <Else ID="1,2,3,4,5,6" />
