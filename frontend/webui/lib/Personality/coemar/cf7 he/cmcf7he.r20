6   61  I  13   100 1N  J  1  "Gobo2"        0    0    0
9   1   I  14   100 1N  M  1  "Effect"       0    0    0
8   1   I  15   100 1N  R  1  "Ef_Spin"      0    0    0
8   61  I  16   100 1N  0  1  "Lens"         0    0    0
10   1  L  17   100 1N  B  1  "Cyan"         0    0    0
10  61  L  18   100 1N  C  1  "Magenta"      0    0    0
