;----------------------------------------------------------------------------
; Macros

MACRO "Reset"         1  16
"data"                0  F8
"delay"               1  46
"data"                0  78
MEND

;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 4  ;Shutter
0   35   "Closed"
36  245  "Strobe %3d%%"(c-a)*100/(b-a)
246 255  "Open"
REND

RANGE 5  ;Pan
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 6  ;Tilt
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 2  ;Colour 1
0   2    "Open"
3   190  "Inde %3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
191 255  "Scroll %3d%%"(c-a)*100/(b-a)
REND

RANGE 3  ;Colour 2
0   30   "Open"
31  190  "Inde %3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
191 255  "Scroll %3d%%"100-((c-a)*100/(b-a))
REND

RANGE 10  ;Gobo 1
0   30   "Open"
31  100  "Index %3d%%"(c-a)*100/(b-a)
101 190  "Index  %3d%%"(c-a)*100/(b-a)
191 255  "Scroll %3d%%"100-((c-a)*100/(b-a))
REND

RANGE 11  ;Gobo 2
0   30   "Open"
31  185  "Inde %3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
186 255  "Scroll %3d%%"100-((c-a)*100/(b-a))
REND

RANGE 12  ;Gobo 2 Rot
0   0    "No Spin"
1   190  "Spin C %3d%%"(c-a)*100/(b-a)
191 255  "Spin C %3d%%"(c-a)*100/(b-a)
REND

RANGE 1  ;Iris
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

RANGE 13  ;Focus
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

RANGE 14  ;Prism
0   125  "Open"
126 255  "Prism"
REND

RANGE 15  ;Prism Rot
0   0    "No Spin"
1   190  "Spin C %3d%%"(c-a)*100/(b-a)
191 255  "Spin C %3d%%"(c-a)*100/(b-a)
REND

RANGE 9  ;P/T Speed
0   255  "%3d%%"100-((c-a)*100/(b-a))
REND

RANGE 16  ;Control
0   240  "Safe"
241 255  "Reset"
REND

