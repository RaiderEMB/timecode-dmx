PFTABLE "Colors" 0    9   7   6   8
"Open"		 1   00  00  00  00
"Red"		 2   FF  00  00  00
"Amber"          3   FF  80  00  00
"Yellow"         4   FF  FF  00  00
"Green"          5   00  FF  00  00
"Cyan"           6   00  FF  FF  00
"Blue"           7   00  00  FF  00
"Lavender"	 8   49  FC  04  00
"Purple"         9   80  00  FF  00
"Magenta"        10  FF  00  FF  00
PFEND

;----------------------------------------------------------------------------
; Macros

MACRO "Reset"         1  10
"data"                0  F8
"delay"               1  28
"data"                0  78
"delay"               1   A
MEND

;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 4  ;Shutter
0   7    "Open"
8   239  "Strobe %3d%%"0+(((c-a)*100)/(b-a))
240 247  "Blackout"
248 255  "Open"
REND

RANGE 5  ;Flash Ratio
0   100  "Length %3d%%"100-(((c-a)*100)/(b-a))
101 127  "Lghtng %3d%%"0+(((c-a)*100)/(b-a))
128 255  "No Flash"
REND

RANGE 10  ;Control
0   239  "Safe"
240 255  "Reset"
REND

