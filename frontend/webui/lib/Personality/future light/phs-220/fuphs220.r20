MEND

MACRO "Lamp Off"      2   9
"data"                0  E8
"delay"               1  50
"data"                0   8
"delay"               1   A
MEND

MACRO "Reset"         3   9
"data"                0  58
"delay"               1  50
"data"                0   8
"delay"               1   A
MEND

