32  55   "E1 CW %3d%%"100-(((c-a)*100)/(b-a))
56  79   "E1 CCW %3d%%"0+(((c-a)*100)/(b-a))
80  103  "E2 CW %3d%%"100-(((c-a)*100)/(b-a))
