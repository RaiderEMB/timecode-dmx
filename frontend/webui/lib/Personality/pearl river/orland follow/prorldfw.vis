Device PR_OrlndFlw

// Iris
ChanRef Iris
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 1
End

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 31 White
    Mapping 32 63 RGB240,0,0
    Mapping 64 95 RGB240,240,0
    Mapping 96 127 RGB120,0,240
    Mapping 128 159 RGB0,240,0
    Mapping 160 191 RGB240,120,0
    Mapping 192 223 RGB0,0,240
    Mapping 224 255 RGB240,0,240
End

// CTO
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 255 255
End

// CTO
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 255 255
End

// CTO
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 255 200
End

// Dimmer/Shutter
ChanRef Dimmer
    Vismode Dimmer
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

