PFTABLE "Pan/Tilt"   -1   1   3
"Middle"             21  80  80
"Left"               22  4F  80
"Right"              23  B0  80
"Top"                24  80  4F
"Bottom"             25  80  B0
"TLeft"              26  4F  4F
"TRight"             27  B0  B0
"BLeft"              28  4F  B0
"BRight"             29  B0  4F
PFEND

;----------------------------------------------------------------------------
; Macros

MACRO "Lamp On"       1  13
"data"                0  9A
"delay"               1  32
"data"                0   4
MEND

MACRO "Lamp Off"      2  13
"data"                0  23
"delay"               1  32
"data"                0   4
MEND

MACRO "Reset Motor"   3  13
"data"                0  DC
"delay"               1  32
"data"                0   4
MEND

MACRO "Reset All"     4  13
"data"                0  F8
"delay"               1  32
"data"                0   4
MEND

