"data"                0  F8
"delay"               1  50
"data"                0   0
MEND

MACRO "Lamp Off"      2   4
"data"                0  D8
"delay"               1  50
"data"                0   0
MEND

MACRO "Reset"         3   4
"data"                0  B8
"delay"               1  50
"data"                0   0
MEND

