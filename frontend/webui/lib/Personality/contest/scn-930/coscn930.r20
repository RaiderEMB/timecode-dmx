MACRO "Lamp On"       1   5
"data"                0  32
"delay"               1  64
"data"                0   A
"delay"               1   A
MEND

MACRO "Lamp Off"      2   5
"data"                0  46
"delay"               1  64
"data"                0   A
"delay"               1   A
MEND

MACRO "Reset"         3   5
"data"                0  5A
"delay"               1  64
"data"                0   A
"delay"               1   A
MEND

