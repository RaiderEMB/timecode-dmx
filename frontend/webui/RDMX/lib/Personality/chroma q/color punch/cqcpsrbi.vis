Device CQ_CPSTRGBI

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 63 1 1
    Mapping 64 127 1 1
    Mapping 128 189 1 1
    Mapping 190 255 30 300
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

//Shutter
ChanRef Dimmer
   Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping if 1 0 63 0 255 100 10
    Mapping if 1 64 127 0 255 10 100
    Mapping if 1 128 189 0 255 -10 -100
End
 
// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

