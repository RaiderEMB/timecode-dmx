  <History Date="22-06-09" Author="Jonathan Moreton" Comment="Checked" />
  <Manual Filename="" Summary="" />
  <Control>
    <Attribute ID="Gobo1Rot" Name="Gobo Rot" Description="" Group="G">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Condition Name="Fixed_Func_Ind" ID="1" If="'Gobo1Func'==1" />
      <Condition Name="Fixed_Func_Spin" ID="2,3,4,5" If="'Gobo1Func'==2" />
      <Else ID="6" />
      <Function ID="1" Name="Index" Display="'Index %.f%%',0~100" Dmx="0~127" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="No Spin" Display="'No Spin'" Dmx="192~192" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="Spin CW" Display="'Spin CW %.f%%',100~0" Dmx="128~191" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="4" Name="No Spin" Display="'No Spin'" Dmx="192~192" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="5" Name="Spin CCW" Display="'Spin CCW %.f%%',0~100" Dmx="193~255" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="6" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Colour1Func" Name="Colour Func" Description="" Group="C" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Fixed_Func" Display="'Fixed'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="2" Name="Split_Func" Display="'Split Gel'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="3" Name="Scroll_Func" Display="'Scroll'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="4" Name="Raw_Func" Display="'Raw DMX'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Colour1" Name="Colour" Description="" Group="C">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed_Func" ID="1,3,5,7,9,11,13,15" If="'Colour1Func'==1" />
      <Condition Name="Split_Func" ID="2,4,6,8,10,12,14" If="'Colour1Func'==2" />
      <Condition Name="Scroll_Func" ID="16,17" If="'Colour1Func'==3" />
      <Else ID="18" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~0" Colour="255,255,255" />
      <Function ID="2" Name="Open/Red" Display="'Open/Red %.f%%',0~100" Dmx="1~12" Colour="255,255,255" />
      <Function ID="3" Name="Red" Display="'Red'" Dmx="13~13" Colour="255,0,0" />
      <Function ID="4" Name="Red/Yellow" Display="'Red / Yellow  %.f%%',0~100" Dmx="14~25" Colour="255,0,0" />
      <Function ID="5" Name="Yellow" Display="'Yellow'" Dmx="26~26" Colour="255,255,0" />
      <Function ID="6" Name="Yellow/UV" Display="'Yellow/UV %.f%%',0~100" Dmx="27~50" Colour="255,255,0" />
      <Function ID="7" Name="UV" Display="'UV'" Dmx="51~51" Colour="180,0,255" />
      <Function ID="8" Name="UV/Green" Display="'UV/Green %.f%%',0~100" Dmx="52~74" Colour="180,0,255" />
      <Function ID="9" Name="Green" Display="'Green'" Dmx="75~75" Colour="0,255,0" />
      <Function ID="10" Name="Green/Orange" Display="'Green / Orange %.f%%',0~100" Dmx="76~86" Colour="0,255,0" />
      <Function ID="11" Name="Orange" Display="'Orange'" Dmx="87~87" Colour="255,128,0" />
      <Function ID="12" Name="Orange/Blue" Display="'Orange / Blue %.f%%',0~100" Dmx="88~100" Colour="255,128,0" />
      <Function ID="13" Name="Blue" Display="'Blue'" Dmx="101~101" Colour="0,0,255" />
      <Function ID="14" Name="Blue/Pink" Display="'Blue/Pink %.f%%',0~100" Dmx="102~126" Colour="0,0,255" />
      <Function ID="15" Name="Pink" Display="'Pink'" Dmx="127~127" Colour="255,150,150" />
      <Function ID="16" Name="No Scroll" Display="'No Scroll'" Dmx="0~0" Colour="255,255,255" />
      <Function ID="17" Name="Scroll" Display="'Scroll %.f%%',0~100" Dmx="128~255" Colour="0,0,0" />
      <Function ID="18" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Gobo1Func" Name="Gobo Func" Description="" Group="G" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Fixed_Func_Ind" Display="'Fixed Index'" Dmx="0~0" Update="Gobo1, Gobo1Rot" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="Fixed_Func_Spin" Display="'Fixed Spin'" Dmx="0~0" Update="Gobo1, Gobo1Rot" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="Raw_Func" Display="'Raw DMX'" Dmx="0~0" Update="Gobo1, Gobo1Rot" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Gobo1" Name="Gobo" Description="" Group="G">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed_Func_Ind" ID="1,2,3,4,5" If="'Gobo1Func'==1||'Gobo1Func'==2" />
      <Else ID="6" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~49" WheelPosition="0" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="Gobo 1" Display="'Gobo 1'" Dmx="50~99" WheelPosition="1" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="Gobo 2" Display="'Gobo 2'" Dmx="100~174" WheelPosition="2" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="4" Name="Gobo 3" Display="'Gobo 3'" Dmx="175~224" WheelPosition="3" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="5" Name="Gobo 4" Display="'Gobo 4'" Dmx="225~255" WheelPosition="4" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="6" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Dimmer" Name="Shutter" Description="" Group="I">
      <Locate Locate="1:100" PowerOn="1:0" />
      <Function ID="1" Name="Dimmer" Display="'%.1f%%',0.0~100.0" Dmx="0~127" Intensity="0~0" />
      <Function ID="2" Name="Strobe %" Display="'Strobe %.f%%',0~100" Dmx="128~254" Intensity="0~0" />
      <Function ID="3" Name="Open" Display="'Open'" Dmx="255~255" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Pan" Name="Pan" Description="" Group="P">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Pan" Display="'%.1f%%',0.0~100.0" Dmx="0~255" />
    </Attribute>
    <Attribute ID="Tilt" Name="Tilt" Description="" Group="P">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Tilt" Display="'%.1f%%',0.0~100.0" Dmx="0~255" />
    </Attribute>
  </Control>
  <Mode Name="6 DMX, 8bit" Channels="6">
    <Import PearlRef="CP_TigerSc" DiamondRef="" WysiwygRef="" />
    <Physical>
      <Bulb Type="" Lumens="" ColourTemp="" />
      <Lens Name="" Degrees="" />
      <Weight Kg="" />
      <Size Height="" Width="" Depth="" />
      <Focus Type="Mirror" PanMax="0" TiltMax="0" PanMaxSpeed="0" TiltMaxSpeed="0" />
    </Physical>
    <Include>
      <Attribute ID="Dimmer" ChannelOffset="4" Wheel="1" />
      <Attribute ID="Pan" ChannelOffset="5" Wheel="2" />
      <Attribute ID="Tilt" ChannelOffset="6" Wheel="3" />
      <Attribute ID="Colour1Func" Wheel="7" />
      <Attribute ID="Colour1" ChannelOffset="2" Wheel="8" />
      <Attribute ID="Gobo1Rot" ChannelOffset="1" Wheel="15" />
      <Attribute ID="Gobo1Func" Wheel="13" />
      <Attribute ID="Gobo1" ChannelOffset="3" Wheel="14" />
    </Include>
  </Mode>
  <Palettes>
    <Group Name="Colour">
      <Palette ID="1" Name="Open" Number="1" Page="1" SetControl="Colour1:1,Colour1Func:1" />
      <Palette ID="2" Name="Red" Number="2" Page="1" SetControl="Colour1:3,Colour1Func:1" />
      <Palette ID="3" Name="Orange" Number="3" Page="1" SetControl="Colour1:11,Colour1Func:1" />
      <Palette ID="4" Name="Yellow" Number="4" Page="1" SetControl="Colour1:5,Colour1Func:1" />
      <Palette ID="5" Name="Yellow" Number="0" Page="1" SetControl="Colour1:5,Colour1Func:1" />
      <Palette ID="6" Name="Green" Number="5" Page="1" SetControl="Colour1:9,Colour1Func:1" />
      <Palette ID="7" Name="Green" Number="0" Page="1" SetControl="Colour1:9,Colour1Func:1" />
      <Palette ID="8" Name="Open" Number="6" Page="1" SetControl="Colour1:1,Colour1Func:1" />
      <Palette ID="9" Name="Blue" Number="0" Page="1" SetControl="Colour1:13,Colour1Func:1" />
      <Palette ID="10" Name="Blue" Number="7" Page="1" SetControl="Colour1:13,Colour1Func:1" />
      <Palette ID="11" Name="UV" Number="9" Page="1" SetControl="Colour1:7,Colour1Func:1" />
      <Palette ID="12" Name="UV" Number="10" Page="1" SetControl="Colour1:7,Colour1Func:1" />
      <Palette ID="13" Name="Red" Number="0" Page="1" SetControl="Colour1:3,Colour1Func:1" />
      <Palette ID="14" Name="UV" Number="8" Page="1" SetControl="Colour1:7,Colour1Func:1" />
      <Palette ID="15" Name="Blue" Number="0" Page="1" SetControl="Colour1:13,Colour1Func:1" />
    </Group>
    <Group Name="Gobo">
      <Palette ID="1" Name="Open" Number="11" Page="1" SetControl="Gobo1:1,Gobo1Func:2" />
      <Palette ID="2" Name="Gobo 1" Number="12" Page="1" SetControl="Gobo1:2,Gobo1Func:2" />
      <Palette ID="3" Name="Gobo 2" Number="13" Page="1" SetControl="Gobo1:3,Gobo1Func:2" />
      <Palette ID="4" Name="Gobo 3" Number="14" Page="1" SetControl="Gobo1:4,Gobo1Func:2" />
      <Palette ID="5" Name="Gobo 4" Number="15" Page="1" SetControl="Gobo1:5,Gobo1Func:2" />
      <Palette ID="14" Name="No Spin" Number="16" Page="1" SetControl="Gobo1Rot:2" />
      <Palette ID="15" Name="Spin CW" Number="17" Page="1" SetControl="Gobo1Rot:3:50" />
      <Palette ID="16" Name="Spin CCW" Number="19" Page="1" SetControl="Gobo1Rot:5:50" />
    </Group>
    <Group Name="Position">
      <Palette ID="1" Name="Middle" Number="21" Page="1" SetControl="Pan:1:50,Tilt:1:50" />
      <Palette ID="2" Name="Left" Number="22" Page="1" SetControl="Pan:1:31,Tilt:1:50" />
      <Palette ID="3" Name="Right" Number="23" Page="1" SetControl="Pan:1:69,Tilt:1:50" />
      <Palette ID="4" Name="Top" Number="24" Page="1" SetControl="Pan:1:50,Tilt:1:31" />
      <Palette ID="5" Name="Bottom" Number="25" Page="1" SetControl="Pan:1:50,Tilt:1:69" />
      <Palette ID="6" Name="TLeft" Number="26" Page="1" SetControl="Pan:1:31,Tilt:1:31" />
      <Palette ID="7" Name="TRight" Number="27" Page="1" SetControl="Pan:1:69,Tilt:1:69" />
      <Palette ID="8" Name="BLeft" Number="28" Page="1" SetControl="Pan:1:31,Tilt:1:69" />
      <Palette ID="9" Name="BRight" Number="29" Page="1" SetControl="Pan:1:69,Tilt:1:31" />
    </Group>
  </Palettes>
</Fixture>