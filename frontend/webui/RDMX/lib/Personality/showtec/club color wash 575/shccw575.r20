"Purple"              9  78   0
"UV"                 10  68   0
"UV"                  8  68   0
PFEND

;----------------------------------------------------------------------------
; Macros

MACRO "Reset"         1   5
"data"                0  86
"delay"               1  32
"data"                0  40
MEND

;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 1  ;Colour 1
0   15   "Open"
16  31   "Red"
32  47   "Blue"
48  63   "Green"
64  79   "CTO"
80  95   "Orange"
96  111  "UV"
112 127  "Purple"
