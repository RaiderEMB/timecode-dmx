"Col 1.8"     9  54
"Col 1.9"    10  5b
PFEND

; Litho table
PFTABLE "Lithos" -1 4
"Open"       11  0
"Col 2.1"     2  20
"Col 2.2"     3  40
"Col 2.3"     4  60
"Col 2.4"     5  80
"Col 2.5"     6  a0
