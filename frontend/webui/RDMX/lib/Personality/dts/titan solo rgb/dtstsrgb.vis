Device DTS_TSRGB

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 9 0 0
    Mapping 10 19 1 1
    Mapping 20 29 0 0
    Mapping 30 119 30 300
    Mapping 120 149 100 200
    Mapping 150 179 200 100
    Mapping 180 229 150 150
    Mapping 230 255 1 1
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

