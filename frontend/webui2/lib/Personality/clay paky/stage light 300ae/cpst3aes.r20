; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 1  ;Colour
0   15   "Open"
16  31   "Red"
32  47   "Yellow"
48  55   "UV"
64  79   "Green"
80  95   "Orange"
96  111  "Blue"
112 127  "Pink"
128 128  "No Scroll"
129 255  "Scroll %3d%%"0+(((c-a)*100)/(b-a))
REND

RANGE 3  ;Gobo
0   36   "Open"
37  73   "Gobo 1"
74  110  "Gobo 2"
111 147  "Gobo 3"
148 184  "Gobo 4"
185 221  "Gobo 5"
222 255  "Gobo 6"
REND

RANGE 4  ;Dimmer
0   139  "%3d%%"0+(((c-a)*100)/(b-a))
140 243  "Strobe %3d%%"0+(((c-a)*100)/(b-a))
244 255  "Open"
REND

RANGE 7  ;Effect
0   0    "Open"
37  37   "Warm"
75  75   "Cold"
112 112  "Light Frost"
147 147  "Heavy Frost"
184 184  "Prism"
219 219  "UV"
255 255  "Light Blue"
REND

RANGE 9  ;Reset
0   127  "Safe"
128 255  "Reset"
REND

RANGE 10  ;Control
0   25   "Safe"
26  100  "Lamp Off"
101 179  "Safe"
180 255  "Lamp On"
REND

