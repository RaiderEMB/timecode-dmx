    <Attribute ID="PanRot" Name="Pan Rotation" Description="" Group="P">
      <Locate Locate="1:50" PowerOn="1:0" />
      <Function ID="1" Name="Safe" Display="'Safe'" Dmx="0~3" />
      <Function ID="2" Name="FW Rotation" Display="'FW Rotation   %.f%%',100~0" Dmx="4~127" />
      <Function ID="3" Name="No Rotation" Display="'No Rotation'" Dmx="128~131" />
      <Function ID="4" Name="Rev Rotation" Display="'Rev Rotation  %.f%%',0~100" Dmx="132~255" />
    </Attribute>
    <Attribute ID="TiltRot" Name="Tilt Rotation" Description="" Group="P">
      <Locate Locate="1:50" PowerOn="1:0" />
