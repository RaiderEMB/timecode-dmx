    <Attribute ID="PT_Speed" Name="P/T Speed" Description="" Group="S">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Tracking" Display="'Tracking'" Dmx="0~10" />
      <Function ID="2" Name="Max" Display="'Max'" Dmx="11~25" />
