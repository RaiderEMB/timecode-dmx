    </Attribute>
    <Attribute ID="Gobo1Rot" Name="Gobo Rot" Description="" Group="G">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Condition Name="Index" ID="1" If="'Gobo1Func'==1||'Gobo1Func'==3" />
      <Condition Name="Spin" ID="2,3,4,5" If="'Gobo1Func'==2||'Gobo1Func'==4" />
