;----------------------------------------------------------------------------
; Macros

MACRO "Reset"         1   5
"data"                0  F4
"delay"               1  14
"data"                0  FC
MEND

MACRO "Full power"    2   5
"data"                0   4
"delay"               1  14
"data"                0  FC
MEND

MACRO "50Hz"          3   5
"data"                0  D4
"delay"               1  14
"data"                0  FC
MEND

MACRO "60Hz"          4   5
"data"                0  DC
"delay"               1  14
"data"                0  FC
MEND

MACRO "Flex"          5   5
"data"                0  E4
"delay"               1  14
"data"                0  FC
MEND

;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 1  ;Pan
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 3  ;Tilt
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 5  ;Control
0   7    "Full power"
8   15   "Fade 8 %3d%%"(c-a)*100/(b-a)
16  23   "Fade 6 %3d%%"(c-a)*100/(b-a)
24  31   "Fade C %3d%%"(c-a)*100/(b-a)
32  207  "Fade %3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
208 215  "50Hz"
216 223  "60Hz"
224 231  "Flex"
240 247  "Reset"
248 255  "Safe"
REND

RANGE 6  ;Shutter
0   15   "Closed"
16  95   "Open"
96  110  "Op Pls %3d%%"(c-a)*100/(b-a)
112 125  "Strobe %3d%%"(c-a)*100/(b-a)
128 142  "Op Pls %3d%%"(c-a)*100/(b-a)
144 158  "Cl Pls %3d%%"(c-a)*100/(b-a)
160 174  "Fade 0"
176 190  "Fade 100"
192 206  "Rnd St %3d%%"(c-a)*100/(b-a)
208 222  "Rnd St %3d%%"(c-a)*100/(b-a)
224 238  "Rnd Fade 0"
240 254  "Rnd Fade 10"
REND

RANGE 7  ;Dimmer
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

RANGE 8  ;Zoom
0   255  "%2d.%d°"12+((c-a)*24/(b-a)),10*(12+((c-a)*24%(b-a)))/(b-a)
REND

RANGE 9  ;Red
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 11  ;Green
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 13  ;Blue
0   255  "%3d.%02d%%"(c-a)*100/(b-a),100*((c-a)*100%(b-a))/(b-a)
REND

RANGE 15  ;CTO
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

