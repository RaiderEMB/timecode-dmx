Device CMCycLedSC

// VisMode Pulse, works like Speed but alternates direction (bounces)
// Place before VisMode Value for backwards compatibility
ChanRef Dimmer
	Vismode Pulse
	UseBits 1 2 3 4 5 6 7 8
	DmxOffset 6
	Mapping 60 108 10 100
	Mapping 111 159 100 10
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 9 1 1
    Mapping 10 57 30 300
    Mapping 58 59 1 1
    Mapping 109 110 1 1
    Mapping 160 161 1 1
    Mapping 162 207 30 300
    Mapping 208 209 1 1
    Mapping 210 255 30 300
End

