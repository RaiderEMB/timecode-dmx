      <Function ID="3" Name="Strobe" Display="'Strobe %.f%%',0~100" Dmx="10~127" Intensity="0~0" />
      <Function ID="4" Name="Random" Display="'Rnd %.f%%',0~100" Dmx="128~247" Intensity="0~0" />
      <Function ID="5" Name="Open" Display="'Open'" Dmx="248~255" Intensity="0~0" />
