      <Function ID="3" Name="P/T Reset" Display="'P/T Reset'" Dmx="30~100" Update="FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens" />
      <Function ID="4" Name="Reset" Display="'Reset'" Dmx="101~170" Update="FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens, FocalLens" />
      <Function ID="5" Name="P/T Soft" Display="'P/T Soft'" Dmx="171~240" Update="FocalLens, FocalLens" />
      <Function ID="6" Name="P/T Std" Display="'P/T Std'" Dmx="240~249" Update="FocalLens, FocalLens" />
