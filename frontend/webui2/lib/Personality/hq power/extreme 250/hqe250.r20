; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 3  ;Colour
0   24   "Open"
25  49   "Red"
50  74   "Yellow"
75  99   "Blue"
100 124  "Green"
125 149  "Mauve"
150 174  "Orange"
175 199  "Pink"
200 255  "Rainb %3d%%"0+(((c-a)*100)/(b-a))
REND

RANGE 4  ;Gobo
0   16    "Closed"
17  33    "Open"
34  50    "Gobo 1"
51  67    "Gobo 2"
68  84    "Gobo 3"
85  101    "Gobo 4"
102 118    "Gobo 5"
119 135    "Gobo 6"
136 152    "Gobo 7"
153 169    "Gobo 8"
170 186    "Gobo 9"
187 199    "Gobo 10"
200 255    "Cycle %3d%%"0+(((c-a)*100)/(b-a))
REND

RANGE 5  ;Control
0   15   "Shtt Opn"
16  91   "Prog 1"
92  135  "Prog 2"
136 195  "Prog 3"
