    <Attribute ID="Prism" Name="Prism" Description="" Group="E">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Condition Name="Index_Func" ID="1,2,3" If="'Prism_Func'==1" />
      <Condition Name="Spin_Func" ID="4,5,6,7,8" If="'Prism_Func'==2" />
