; 'Varilite VL 2402', 10 DMX Channels, filename: VL2402M1.R20
;
;   Date       By                Description
;   26-08-04   Pers Creator      Created - Exported from Diamond 4 file.
;   26-08-04   Tom               Checked.
;   14-11-05   Ed Spencer  	 Moved Control to bank 20
;
;----------------------------------------------------------------------------
; The device name, upto 11 characters, no spaces are allowed
DEVICE  VL2402M1                              mode=Standard 8bit  10 DMX Channels
NAME "Varilite" "VL2402, 10 DMX"
;
;----------------------------------------------------------------------------
; The Type of instrument, 1 & M must required for backward compatibility
; The middle number denotes the number of DMX channels used
TYPE  1  10  M
;
;----------------------------------------------------------------------------
; Mirror/Head resolution
MIRROR  0   0   0
;
;----------------------------------------------------------------------------
; Patching Information. (Upto 10 lines allowed)
DEVICEADDRESS
"Handle  %2d"h
"Fixture %11s"n
"DMX     %03d"d
""
""
"Set to Standard 8bit"
""
""
DAEND
;
;----------------------------------------------------------------------------
; Specify DMX limits
;
; minimum allowed dmx number
MINDMX=1
;
; maximum allowed dmx number
MAXDMX=502
;
; DMX number must be divisible by this
DIVBY=1
;
;----------------------------------------------------------------------------
; DMX channel description
;
DMX
1     1   H    1   100   1N   H   1   "Dimmer"          255     0     0
2    61   L    9   100   1N   O   1   "Shutter"           0     0     0
3    61   L    2   100   1N   E   1   "Pan"             127     0     0
3     1   L    3   100   1N   F   1   "Tilt"            127     0     0
10    1   L    4   100   1N   B   1   "Cyan"              0     0     0
9    61   L    5   100   1N   D   1   "Yellow"            0     0     0
10   61   L    6   100   1N   C   1   "Magenta"           0     0     0
4    61   I    7   100   1N   A   1   "Colour"            0     0     0
5    61   L    8   100   1N   L   1   "Frost"             0     0     0
20    1   I   10   100   1N   0   1   "Control"           0     0     0
END
;
;----------------------------------------------------------------------------
TEXT
Varilite VL 2402
Standard 8bit

INPUT fixture DMX No.
Press a Fixture
Select or Swop button
to make the patch.
END

ATTR
Function Obsolete
END

;----------------------------------------------------------------------------
; Preset Palette Tables

;----------------------------------------------------------------------------
; Macros

MACRO "Lamp On"       1  10
"data"                0   0
"delay"               1   A
"data"                0  FC
"delay"               1  32
"data"                0   0
MEND

MACRO "Lamp Off"      2  10
"data"                0   0
"delay"               1   A
"data"                0  A8
"delay"               1  32
"data"                0   0
MEND

MACRO "Reset"         3  10
"data"                0   0
"delay"               1   A
"data"                0  54
"delay"               1  32
"data"                0   0
MEND
