; 'Robe ColorWash 250 AT', 18 DMX Channels, filename: ROCW25M4.R96
;
;   Date       By                Description
;   11-05-07   D.Milligan        Created from R20 file.
;
;----------------------------------------------------------------------------
; The device name, upto 11 characters, no spaces are allowed
DEVICE  RO_WS250AT4                          mode=Mode 4, 16bit  18 DMX Channels
NAME "Robe" "ColorWash 250 AT, Mode 4"
;
;----------------------------------------------------------------------------
; The Type of instrument, 1 & M must required for backward compatibility
; The middle number denotes the number of DMX channels used
TYPE  1  18  M
;
;----------------------------------------------------------------------------
; Mirror/Head resolution
MIRROR  2  530   280
;       |    |    |
;       |    |    |_______ maximum tilt movement (in degrees)
;       |    |____________ maximum pan movement (in degrees)
;       |_________________ 1 means moving mirror, 2 means moving head
;
;----------------------------------------------------------------------------
DEVICEADDRESS
"Handle  %2d"h
"Fixture %11s"n
"DMX     %03d"d
""
""
"Set to Mode 4, 16bit"
""
""
DAEND
;
;----------------------------------------------------------------------------
; Specify DMX limits
;
; minimum allowed dmx number
MINDMX=1
;
; maximum allowed dmx number
MAXDMX=494
;
; DMX number must be divisible by this
DIVBY=1
;
;----------------------------------------------------------------------------
; DMX channel description
;
DMX
2    61   L   17   100   1N   O   1   "Shutter"          32     0     0
1     1   H   18   100   1N   H   1   "Dimmer"          255     0     0
3    61   s    1   100   1N   E   1   "Pan"             128     0     0
0     0   L    2   100   1N   E   1   "Pan Low"         128     0     0
3     1   s    3   100   1N   F   1   "Tilt"            128     0     0
0     0   L    4   100   1N   F   1   "Tilt Low"        128     0     0
4    61   I    9   100   1N   A   1   "Colour"          128     0     0
10    1   L   10   100   1N   B   1   "Cyan"              0     0   255
10   61   L   11   100   1N   C   1   "Magenta"           0     0   255
9    61   L   12   100   1N   D   1   "Yellow"            0   255    51
4     1   I   14   100   1N   N   1   "Colour Macro"      0     0     0
5     1   L   16   100   1N   Q   1   "Zoom"            128     0     0
9     1   I   15   100   1N   L   1   "Effect"            0     0     0
8    61   L    5   100   1N   0   1   "P/T Speed"         0     0     0
20   61   I    6   100   1N   0   1   "Control"           0     0     0
18   61   I    7   100   1N   0   1   "P/T Macro"         0     0     0
18    1   L    8   100   1N   0   1   "P/T Macro Sp"      0     0     0
8     1   L   13   100   1N   0   1   "Speed"             0     0     0
END
;
;----------------------------------------------------------------------------
TEXT
Robe ColorWash 250 AT
Mode 4, 16bit

INPUT fixture DMX No.
Press a Fixture
Select or Swop button
to make the patch.
END

ATTR
Function Obsolete
END

