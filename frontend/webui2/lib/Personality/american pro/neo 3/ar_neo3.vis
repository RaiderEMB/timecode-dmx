Device AR_Neo3

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 15 White
    Mapping 16 31 Red
    Mapping 32 47 Green
    Mapping 48 63 Blue
    Mapping 64 79 Orange
    Mapping 80 95 RGB220,220,240
    Mapping 96 111 RGB240,240,200
    Mapping 112 127 RGB180,0,240
End

// Colour
ChanRef Color Wheel 1
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 128 189 50 5
    Mapping 192 255 -5 -50
End

// Cyan
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 255 0
End

// Magenta
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 255 0
