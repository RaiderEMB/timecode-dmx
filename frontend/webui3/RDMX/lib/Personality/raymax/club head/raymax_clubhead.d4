    </Attribute>
    <Attribute ID="Control" Name="Control" Description="" Group="S">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="Off" Display="'Off'" Dmx="0~0" />
      <Function ID="2" Name="On" Display="'On'" Dmx="255~255" />
    </Attribute>
