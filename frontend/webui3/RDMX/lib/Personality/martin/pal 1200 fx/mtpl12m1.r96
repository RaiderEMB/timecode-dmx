; Tables

; Positions
PFTABLE "Positions" -1  15 16
"Middle"    21  80  80   
"Left"      22  50  80   
"Right"     23  b0  80
"Top"       24  80  50
"Bottom"    25  80  b0
"TLeft"     26  50  50
"TRight"    27  b0  b0
"BLeft"     28  50  b0
"BRight"    29  b0  50
PFEND

; A color table color wheel, cyan, magenta, yellow
PFTABLE "Color" -1   3   4   5   6
"White"          1  ff  ff  ff   0
"Red"            2  ff   0   0   0
"Amber"          3  ff  80   0   0
"Yellow"         4  ff  ff   0   0
"Green"          5  ff   0   0   0
"Cyan"           6   0  ff  ff   0
"Blue"           7   0  5a  ff   0
"Lavender"       8  69  5a  ff   0
"UV"             9   0  69  ff   0
"Magenta"       10  ff   0  ff   0
PFEND

; Gobo table
PFTABLE "Gobos" -1   7
"Open"		11  bc
"Gobo 1"        12  a4
"Gobo 2"        13  a8
"Gobo 3"        14  ac
"Gobo 4"        15  b0
"Gobo 5"        16  b4
"Gobo 6"        17  b8
PFEND

MACRO "RESET" 1   1
"data"        0  d5
"delay"       1  50
"data"        0   0
MEND

MACRO  "LAMP ON"  2 1
"data"           0  e9
"delay"          1  50
"data"           0   0
MEND

MACRO "LAMP OFF" 3    1 12 13 14
"data"           0   ff e6 e7 e8
"delay"          1   60
"data"           0    0  0  0  0
MEND
