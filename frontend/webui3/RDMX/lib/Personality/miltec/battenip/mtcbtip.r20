RANGE 1  ;Chases
0   3    "Lamp Off"
4   9    "Lamp On"
10  39   "Strobe %3d%%"0+(((c-a)*100)/(b-a))
40  69   "Fd In/ %3d%%"0+(((c-a)*100)/(b-a))
70  99   "Auto C %3d%%"0+(((c-a)*100)/(b-a))
100 110  "Chase 1"
111 121  "Chase 2"
122 132  "Chase 3"
133 143  "Chase 4"
144 154  "Chase 5"
155 165  "Chase 6"
166 176  "Chase 7"
177 187  "Chase 8"
188 198  "Chase 9"
199 209  "Chase 10"
210 220  "Chase 11"
221 231  "Chase 12"
232 244  "Auto Mac"
245 250  "Snd Chase"
251 255  "Snd Strb"
REND

