PFTABLE "Positions" -1 5 6
"Middle" 21 80 80
"Left" 22 50 80
"Right" 23 b0 80
"Top" 24 80 50
"Bottom" 25 80 b0
"TLeft" 26 50 50
"TRight" 27 b0 b0
"BLeft" 28 50 b0
"BRight" 29 b0 50
PFEND

; A color table,
PFTABLE "Color" 29       1
"White" 1 0
"Red" 2 28
"Yellow" 3 48
"Violet" 4 68
"Green" 5 88
"Orange" 6 a8
"Blue" 7 c8
"Pink" 8 d8
