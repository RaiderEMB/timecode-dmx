    </Attribute>
    <Attribute ID="Shutter" Name="Shutter" Description="" Group="I">
      <Locate Locate="3" PowerOn="3" />
      <Function ID="1" Name="Closed" Display="'Closed'" Dmx="0~3" Intensity="0~0" />
      <Function ID="2" Name="Strobe" Display="'Strobe %.f%%',0~100" Dmx="4~103" Intensity="0~0" />
      <Function ID="3" Name="Open" Display="'Open'" Dmx="104~107" Intensity="0~0" />
      <Function ID="4" Name="Pulse" Display="'Pulse %.f%%',0~100" Dmx="108~207" Intensity="0~0" />
      <Function ID="5" Name="Open" Display="'Open'" Dmx="208~212" Intensity="0~0" />
      <Function ID="6" Name="Rnd Strobe" Display="'Rnd Strobe    %.f%%',0~100" Dmx="213~251" Intensity="0~0" />
      <Function ID="7" Name="Open" Display="'Open'" Dmx="252~255" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Reset" Name="Reset" Description="" Group="S">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="Safe" Display="'Safe'" Dmx="0~25" />
      <Function ID="2" Name="Reset_Effects" Display="'Reset Effects'" Dmx="26~76" />
      <Function ID="3" Name="Reset_PT" Display="'Reset PT'" Dmx="77~127" />
      <Function ID="4" Name="Reset_All" Display="'Reset All'" Dmx="128~255" />
    </Attribute>
    <Attribute ID="Control" Name="Control" Description="" Group="S">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="Safe" Display="'Safe'" Dmx="0~25" />
      <Function ID="2" Name="Lamp Off" Display="'Lamp Off'" Dmx="26~100" />
      <Function ID="3" Name="Half Lamp" Display="'Half Lamp'" Dmx="101~179" />
      <Function ID="4" Name="Lamp On" Display="'Lamp On'" Dmx="180~255" />
    </Attribute>
    <Attribute ID="Col_RainON_Func" Name="Colour Func" Description="" Group="C" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Fixed_Func" Display="'Fixed'" Dmx="0~0" Update="Col_RainON" Colour="0,0,0" />
      <Function ID="2" Name="Scroll_Func" Display="'Scroll'" Dmx="0~0" Update="Col_RainON" Colour="0,0,0" />
      <Function ID="3" Name="Raw_Func" Display="'Raw DMX'" Dmx="0~0" Update="Col_RainON" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Col_RainON" Name="Colour" Description="" Group="C">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed_Func" ID="1,2,3,4,5" If="'Col_RainON_Func'==1" />
      <Condition Name="Scroll_Func" ID="6,7" If="'Col_RainON_Func'==2" />
      <Else ID="8" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~24" Colour="255,255,255" />
      <Function ID="2" Name="CTB" Display="'CTB'" Dmx="25~49" Colour="230,230,255" />
      <Function ID="3" Name="Red" Display="'Red'" Dmx="50~74" Colour="255,0,0" />
      <Function ID="4" Name="Blue" Display="'Blue'" Dmx="75~99" Colour="0,0,255" />
      <Function ID="5" Name="UV" Display="'UV'" Dmx="100~127" Colour="180,0,255" />
      <Function ID="6" Name="No Scroll" Display="'No Scroll'" Dmx="0~24" Colour="255,255,255" />
      <Function ID="7" Name="Scroll" Display="'Scroll %.f%%',0~100" Dmx="128~255" Colour="0,0,0" />
      <Function ID="8" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Col_RainOFF_Func" Name="Colour Func" Description="" Group="C" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Fixed_Func" Display="'Fixed'" Dmx="0~0" Update="Col_RainOFF" Colour="0,0,0" />
      <Function ID="2" Name="Raw_Func" Display="'Raw DMX'" Dmx="0~0" Update="Col_RainOFF" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Col_RainOFF" Name="Colour" Description="" Group="C">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed_Func" ID="1,2,3,4,5" If="'Col_RainOFF_Func'==1" />
      <Else ID="1,6" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~50" Colour="255,255,255" />
      <Function ID="2" Name="CTB" Display="'CTB'" Dmx="51~101" Colour="230,230,255" />
      <Function ID="3" Name="Red" Display="'Red'" Dmx="102~152" Colour="255,0,0" />
      <Function ID="4" Name="Blue" Display="'Blue'" Dmx="153~203" Colour="0,0,255" />
      <Function ID="5" Name="UV" Display="'UV'" Dmx="204~255" Colour="180,0,255" />
      <Function ID="6" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" Colour="0,0,0" />
    </Attribute>
  </Control>
