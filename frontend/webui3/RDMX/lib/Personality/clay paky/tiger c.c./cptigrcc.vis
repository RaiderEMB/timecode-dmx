Device CP_TIGERCC

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 35 White
    Mapping 36 71 RGB240,0,0
    Mapping 72 107 RGB240,240,0
    Mapping 108 143 RGB180,0,180
    Mapping 144 179 RGB0,240,0
    Mapping 180 215 RGB240,120,0
    Mapping 216 254 RGB0,0,240
    Mapping 255 255 RGB240,140,140
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 127 0 255
    Mapping 128 255 255 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 139 1 1
    Mapping 140 243 30 300
    Mapping 244 255 1 1
End

