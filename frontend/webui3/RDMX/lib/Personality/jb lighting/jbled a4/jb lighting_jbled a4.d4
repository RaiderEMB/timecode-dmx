      <Function ID="1" Name="Blue C-Mix" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" Colour="0,0,255" />
    </Attribute>
    <Attribute ID="CTO" Name="CTO" Description="" Group="C">
