      <Function ID="3" Name="Strobe %" Display="'Strobe %.f%%',0~100" Dmx="112~125" Intensity="0~0" />
      <Function ID="4" Name="Rnd Strb 0%" Display="'Rnd Strb 0 %.f%%',0~100" Dmx="208~222" Intensity="0~0" />
      <Function ID="5" Name="Rnd Strb 100%" Display="'Rnd Strb 100 %.f%%',0~100" Dmx="192~206" Intensity="0~0" />
      <Function ID="6" Name="Op Pls &gt;10Hz" Display="'Op Pls &gt;10Hz %.f%%',0~100" Dmx="96~110" Intensity="0~0" />
      <Function ID="7" Name="Op Pls &lt;10Hz" Display="'Op Pls &lt;10Hz %.f%%',0~100" Dmx="128~142" Intensity="0~0" />
      <Function ID="8" Name="Cl Pls" Display="'Cl Pls %.f%%',0~100" Dmx="144~158" Intensity="0~0" />
      <Function ID="9" Name="Fade 0%" Display="'Fade 0'" Dmx="160~174" Intensity="0~0" />
      <Function ID="10" Name="Fade 100%" Display="'Fade 100'" Dmx="176~190" Intensity="0~0" />
      <Function ID="11" Name="Rnd Fade 0%" Display="'Rnd Fade 0'" Dmx="224~238" Intensity="0~0" />
