      <Function ID="5" Name="Fade C-Picker" Display="'Fade C-Picker %.f%%',0~100" Dmx="24~31" />
      <Function ID="6" Name="Fade Safe" Display="'Fade Safe %.1f%%',0.0~100.0" Dmx="32~207" />
