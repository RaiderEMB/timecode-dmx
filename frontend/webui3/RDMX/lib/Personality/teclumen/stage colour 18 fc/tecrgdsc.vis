    DmxOffset 1
    Mapping 0 255 0 255
End

// Red
ChanRef Red
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 1 255 5 50
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 1 255 2 70
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
