"data"                0  FF
"delay"               1  3C
"data"                0  FF
MEND

MACRO "Lamp Off"      2  12   9
"data"                0  F0   0
"delay"               1  3C
"data"                0   0   0
"delay"               1   A
MEND

MACRO "Reset"         3  12
"data"                0  FC
"delay"               1  3C
"data"                0   0
"delay"               1   A
MEND

