Device PZ_STLRGB14

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

//Shutter
Chanref Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 3 0 0
    Mapping 4 9 1 1
    Mapping 10 39 30 300
End

// Red Seg 1
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Red Seg 1
ChanRef Red
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 100 255 -50 -5
End

// Green Seg 1
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Green Seg 1
ChanRef Green
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 100 255 2 70 
End

// Blue Seg 1
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

// Blue Seg 1
ChanRef Blue
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 100 255 5 50
End

