      <Function ID="7" Name="Clock 3" Display="'Clock 3'" Dmx="85~99" Update="Red, Green, Blue, Orange" />
      <Function ID="8" Name="Fan 1" Display="'Fan 1'" Dmx="100~114" Update="Red, Green, Blue, Orange" />
