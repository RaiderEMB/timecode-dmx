      <Function ID="2" Name="Clock 1" Display="'Clock 1'" Dmx="11~24" Update="Red, Green, Blue, Orange" />
      <Function ID="3" Name="Clock 2" Display="'Clock 2'" Dmx="25~39" Update="Red, Green, Blue, Orange" />
