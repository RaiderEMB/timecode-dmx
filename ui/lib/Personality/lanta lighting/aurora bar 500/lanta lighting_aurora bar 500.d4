    </Attribute>
    <Attribute ID="Shutter1" Name="Shutter" Description="" Group="I">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~0" Intensity="0~0" />
      <Function ID="2" Name="Closed" Display="'Closed'" Dmx="1~5" Intensity="0~0" />
      <Function ID="3" Name="Strobe %" Display="'Strobe %.f%%',0~100" Dmx="6~255" Intensity="0~0" />
    </Attribute>
  </Control>
