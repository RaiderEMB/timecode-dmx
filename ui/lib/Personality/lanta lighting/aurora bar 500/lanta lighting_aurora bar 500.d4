      <Function ID="6" Name="Green Fade" Display="'Green Fade'" Dmx="204~254" />
      <Function ID="7" Name="Blue Fade" Display="'Blue Fade'" Dmx="255~255" />
    </Attribute>
    <Attribute ID="Speed" Name="Speed" Description="" Group="S">
      <Locate Locate="1:100" PowerOn="1:100" />
      <Function ID="1" Name="Generic %" Display="'%.f%%',0~100" Dmx="0~255" />
    </Attribute>
