  <Control>
    <Attribute ID="Control" Name="Control" Description="" Group="S">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="No Macro" Display="'No Macro'" Dmx="0~0" />
      <Function ID="2" Name="Colour Change" Display="'Colour Change'" Dmx="1~50" />
      <Function ID="3" Name="Jumping Colours" Display="'Jumping       Colours'" Dmx="51~101" />
      <Function ID="4" Name="Dreaming Colours" Display="'Dreaming      Colours'" Dmx="102~152" />
