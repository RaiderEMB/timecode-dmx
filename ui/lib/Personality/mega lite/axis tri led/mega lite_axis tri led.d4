  <History Date="04-08-09" Author="David Hunt" Comment="Created" />
  <History Date="25-08-09" Author="Jonathan Moreton" Comment="Checked" />
  <History Date="24-03-11" Author="Nic Morris" Comment="Changed manufacturer from Unknown." />
  <History Date="24-03-11" Author="Nic Morris" Comment="Increased wheel resolution.  Standardised func names.  Fixed power on and locate." />
  <History Date="24-03-11" Author="Nic Morris" Comment="Inverted ranges for logical wheel direction.  Added Macro." />
  <History Date="24-03-11" Author="Nic Morris" Comment="Adapted from Axis LED" />
  <Manual Filename="" Summary="" />
  <Control>
    <Attribute ID="Pan" Name="Pan" Description="" Group="P" Size="2">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Pan" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" />
    </Attribute>
    <Attribute ID="Tilt" Name="Tilt" Description="" Group="P" Size="2">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Tilt" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" />
    </Attribute>
    <Attribute ID="PT_Speed" Name="P/T Speed" Description="" Group="S">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Max Speed" Display="'Max Speed'" Dmx="0~0" />
      <Function ID="2" Name="Speed" Display="'%.1f%%',0.0~100.0" Dmx="244~1" />
      <Function ID="3" Name="Changes" Display="'Changes'" Dmx="245~252" />
      <Function ID="4" Name="Changes w P/T" Display="'Changes w P/T'" Dmx="253~255" />
    </Attribute>
    <Attribute ID="Control" Name="Control" Description="" Group="S">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Safe" Display="'Safe'" Dmx="151~255" />
      <Function ID="2" Name="Fan Speed" Display="'Fan Speed %.1f%%',0.0~100.0" Dmx="127~0" />
      <Function ID="3" Name="Reset" Display="'Reset'" Dmx="140~150" />
    </Attribute>
    <Attribute ID="Red" Name="Red" Description="" Group="C">
      <Locate Locate="1:100" PowerOn="1:0" Highlight="1:100" Lowlight="1:0" />
      <Function ID="1" Name="Red C-Mix" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Colour="255,0,0" />
    </Attribute>
    <Attribute ID="Green" Name="Green" Description="" Group="C">
      <Locate Locate="1:100" PowerOn="1:0" Highlight="1:100" Lowlight="1:0" />
      <Function ID="1" Name="Green C-Mix" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Colour="0,255,0" />
    </Attribute>
    <Attribute ID="Blue" Name="Blue" Description="" Group="C">
      <Locate Locate="1:100" PowerOn="1:0" Highlight="1:0" Lowlight="1:90" />
      <Function ID="1" Name="Blue C-Mix" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Colour="0,0,255" />
    </Attribute>
    <Attribute ID="Shutter" Name="Shutter" Description="" Group="I">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="Strobe %" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Colour1Func" Name="Colour Func" Description="" Group="C" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Fixed" Display="'Fixed'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="2" Name="Pulse" Display="'Pulse'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="3" Name="Scroll" Display="'Scroll'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="4" Name="Raw DMX" Display="'Raw DMX'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Colour1" Name="Colour" Description="" Group="C">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed" ID="1,2,3,4,5,6,7" If="'Colour1Func'==1" />
      <Condition Name="Scroll" ID="8,9" If="'Colour1Func'==3" />
      <Condition Name="Pulse" ID="10,11,12,13,14,15,16" If="'Colour1Func'==2" />
      <Else ID="17" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~0" Colour="255,255,255" />
      <Function ID="2" Name="Red" Display="'Red'" Dmx="1~19" Colour="255,0,0" />
      <Function ID="3" Name="Green" Display="'Green'" Dmx="20~39" Colour="0,255,0" />
      <Function ID="4" Name="Blue" Display="'Blue'" Dmx="40~59" Colour="0,0,255" />
      <Function ID="5" Name="Colour Fade" Display="'Colour Fade %.f%%',0~100" Dmx="60~79" Colour="255,255,0" />
      <Function ID="6" Name="Colour Snap" Display="'Colour Snap %.f%%',0~100" Dmx="80~99" Colour="128,0,255" />
      <Function ID="7" Name="Rnd Col Fade" Display="'Rnd Col Fade %.f%%',0~100" Dmx="100~119" Colour="0,255,255" />
      <Function ID="8" Name="No Scroll" Display="'No Scroll'" Dmx="0~0" Colour="255,255,255" />
      <Function ID="9" Name="Fast Scroll" Display="'Fast Scroll'" Dmx="120~139" Colour="255,255,255" />
      <Function ID="10" Name="Red Pulse" Display="'Red Pulse'" Dmx="140~159" Colour="255,0,0" />
      <Function ID="11" Name="Green Pulse" Display="'Green Pulse'" Dmx="160~179" Colour="0,255,0" />
      <Function ID="12" Name="Blue Pulse" Display="'Blue Pulse'" Dmx="180~199" Colour="0,0,255" />
      <Function ID="13" Name="Yellow Pulse" Display="'Yellow Pulse'" Dmx="200~209" Colour="255,255,0" />
      <Function ID="14" Name="Purple Pulse" Display="'Purple Pulse'" Dmx="210~219" Colour="128,0,255" />
      <Function ID="15" Name="Aqua Pulse" Display="'Aqua Pulse'" Dmx="220~229" Colour="0,255,255" />
      <Function ID="16" Name="White Pulse" Display="'White Pulse'" Dmx="230~255" Colour="255,255,255" />
      <Function ID="17" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Dimmer" Name="Dimmer" Description="" Group="I">
      <Locate Locate="1:100" PowerOn="1:0" />
      <Function ID="1" Name="Dimmer" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Intensity="0~0" />
    </Attribute>
  </Control>
  <Mode Name="12 DMX, 16bit" Channels="12">
    <Import PearlRef="MGL_AXISTRI" DiamondRef="" WysiwygRef="" />
    <Physical>
      <Bulb Type="" Lumens="" ColourTemp="" />
      <Lens Name="" Degrees="" />
      <Weight Kg="" />
      <Size Height="" Width="" Depth="" />
      <Focus Type="Head" PanMax="0" TiltMax="0" PanMaxSpeed="0" TiltMaxSpeed="0" />
    </Physical>
    <Include>
      <Attribute ID="Shutter" ChannelOffset="10" Wheel="4" />
      <Attribute ID="Dimmer" ChannelOffset="12" Wheel="1" />
      <Attribute ID="Pan" ChannelOffset="1,3" Wheel="2" />
      <Attribute ID="Tilt" ChannelOffset="2,4" Wheel="3" />
      <Attribute ID="Red" ChannelOffset="7" Wheel="10" />
      <Attribute ID="Green" ChannelOffset="8" Wheel="11" />
      <Attribute ID="Blue" ChannelOffset="9" Wheel="12" />
      <Attribute ID="Colour1Func" Wheel="7" />
      <Attribute ID="Colour1" ChannelOffset="11" Wheel="8" />
      <Attribute ID="PT_Speed" ChannelOffset="5" Wheel="25" />
      <Attribute ID="Control" ChannelOffset="6" Wheel="28" />
    </Include>
  </Mode>
  <Macro>
    <Macro Name="Reset">
      <Set Channel="Control:3" Delay="4000" />
      <Set Channel="Control:1" Delay="0" />
    </Macro>
  </Macro>
  <Palettes>
    <Group Name="Colour">
      <Palette ID="1" Name="Open" Number="1" Page="1" SetControl="Red:1:100,Green:1:100,Blue:1:100" />
      <Palette ID="2" Name="Red" Number="2" Page="1" SetControl="Red:1:100,Green:1:0,Blue:1:0" />
      <Palette ID="3" Name="Amber" Number="3" Page="1" SetControl="Red:1:100,Green:1:50,Blue:1:0" />
      <Palette ID="4" Name="Yellow" Legend="Yell" Number="4" Page="1" SetControl="Red:1:100,Green:1:100,Blue:1:0" />
      <Palette ID="5" Name="Lime" Number="0" Page="1" SetControl="Red:1:50,Green:1:100,Blue:1:0" />
      <Palette ID="6" Name="Green" Number="5" Page="1" SetControl="Red:1:0,Green:1:100,Blue:1:0" />
      <Palette ID="7" Name="Aqua" Number="0" Page="1" SetControl="Red:1:0,Green:1:100,Blue:1:50" />
      <Palette ID="8" Name="Cyan" Number="6" Page="1" SetControl="Red:1:0,Green:1:100,Blue:1:100" />
      <Palette ID="9" Name="Light Blue" Number="0" Page="1" SetControl="Red:1:0,Green:1:50,Blue:1:100" />
      <Palette ID="10" Name="Blue" Number="7" Page="1" SetControl="Red:1:0,Green:1:0,Blue:1:100" />
      <Palette ID="11" Name="Purple" Legend="Purpl" Number="9" Page="1" SetControl="Red:1:50,Green:1:0,Blue:1:100" />
      <Palette ID="12" Name="Magenta" Legend="Mag" Number="10" Page="1" SetControl="Red:1:100,Green:1:0,Blue:1:100" />
      <Palette ID="13" Name="Rose" Number="0" Page="1" SetControl="Red:1:100,Green:1:0,Blue:1:50" />
      <Palette ID="14" Name="Lavender" Legend="Lav" Number="8" Page="1" SetControl="Red:1:71,Green:1:43,Blue:1:94" />
      <Palette ID="15" Name="Ultra Violet" Legend="UV" Number="0" Page="1" SetControl="Red:1:20,Green:1:0,Blue:1:100" />
    </Group>
    <Group Name="Position">
      <Palette ID="1" Name="Middle" Number="21" Page="1" SetControl="Pan:1:50,Tilt:1:50" />
      <Palette ID="2" Name="Left" Number="22" Page="1" SetControl="Pan:1:31,Tilt:1:50" />
      <Palette ID="3" Name="Right" Number="23" Page="1" SetControl="Pan:1:69,Tilt:1:50" />
      <Palette ID="4" Name="Top" Number="24" Page="1" SetControl="Pan:1:50,Tilt:1:31" />
      <Palette ID="5" Name="Bottom" Number="25" Page="1" SetControl="Pan:1:50,Tilt:1:69" />
      <Palette ID="6" Name="TLeft" Number="26" Page="1" SetControl="Pan:1:31,Tilt:1:31" />
      <Palette ID="7" Name="TRight" Number="27" Page="1" SetControl="Pan:1:69,Tilt:1:69" />
      <Palette ID="8" Name="BLeft" Number="28" Page="1" SetControl="Pan:1:31,Tilt:1:69" />
      <Palette ID="9" Name="BRight" Number="29" Page="1" SetControl="Pan:1:69,Tilt:1:31" />
    </Group>
  </Palettes>
</Fixture>