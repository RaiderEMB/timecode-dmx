Device MGL_NECPS_F

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 9 1 1
    Mapping 10 57 30 300
    Mapping 58 59 1 1
    Mapping 109 110 1 1		
    Mapping 160 161 1 1
    Mapping 162 255 30 300
End

// Shutter
ChanRef Dimmer
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
	Mapping 60 108 10 100			//Open Pulse
	Mapping 111 159 -10 -100			//Close Pulse
End

