Device QM_QS250Sm2

// VisMode Pulse, works like Speed but alternates direction (bounces)
// Place before VisMode Value for backwards compatibility
ChanRef Dimmer
	Vismode Speed
	UseBits 1 2 3 4 5 6 7 8
	DmxOffset 3
    Mapping 76 107 10 100	//Open Pulse
    Mapping 108 139 -10 -100	//Close Pulse
    Mapping 190 219 10 100	//Rnd Open Pulse
    Mapping 220 249 -10 -100	//Rnd Close Pulse
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 9 0 0
    Mapping 10 19 1 1
    Mapping 20 65 30 300		//Strobe
    Mapping 66 75 1 1
    Mapping 140 149 1 1
    Mapping 150 179 30 300		//Random Strobe
    Mapping 180 189 1 1
    Mapping 250 255 1 1
End

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 -270.000000 270.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 -125.000000 125.000000
End

