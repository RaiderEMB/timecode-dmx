  <Manual Filename="/manual/Clay Paky_Stage Light 300.pdf" Summary="Creator:Clay Paky, 09-07-2001" />
  <Control>
    <Attribute ID="Colour1" Name="Colour" Description="" Group="C">
      <Locate Locate="1" PowerOn="1" Highlight="5" Lowlight="13" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~0" Colour="255,255,255" />
      <Function ID="2" Name="Open-Red" Display="'Open-Red'" Dmx="8~15" Colour="0,0,0" />
      <Function ID="3" Name="Red" Display="'Red'" Dmx="16~23" Colour="255,0,0" />
      <Function ID="4" Name="Red-Yellow" Display="'Red-Yellow'" Dmx="24~31" Colour="0,0,0" />
      <Function ID="5" Name="Yellow" Display="'Yellow'" Dmx="32~39" Colour="255,255,0" />
      <Function ID="6" Name="Yellow-Violet" Display="'Yellow-Violet'" Dmx="40~47" Colour="0,0,0" />
      <Function ID="7" Name="Violet" Display="'Violet'" Dmx="48~55" Colour="128,0,128" />
      <Function ID="8" Name="Violet-Green" Display="'Violet-Green'" Dmx="56~63" Colour="0,0,0" />
      <Function ID="9" Name="Green" Display="'Green'" Dmx="64~71" Colour="0,255,0" />
      <Function ID="10" Name="Green-Orange" Display="'Green-Orange'" Dmx="72~79" Colour="0,0,0" />
      <Function ID="11" Name="Orange" Display="'Orange'" Dmx="80~87" Colour="255,128,0" />
      <Function ID="12" Name="Orange-Blue" Display="'Orange-Blue'" Dmx="88~95" Colour="0,0,0" />
      <Function ID="13" Name="Blue" Display="'Blue'" Dmx="96~103" Colour="0,0,255" />
      <Function ID="14" Name="Blue-Pink" Display="'Blue-Pink'" Dmx="104~111" Colour="0,0,0" />
      <Function ID="15" Name="Pink" Display="'Pink'" Dmx="112~119" Colour="255,128,255" />
      <Function ID="16" Name="Pink-White" Display="'Pink-White'" Dmx="120~127" Colour="0,0,0" />
      <Function ID="17" Name="Spin" Display="'%.2f RPM',0.00~300.00" Dmx="128~255" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Gobo1Rot" Name="Gobo 1 Rot" Description="" Group="G">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Index 0°" Display="'Index 0°'" Dmx="0~0" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="Index 0-90" Display="'Index %.f°',1~89" Dmx="1~20" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="Index 90°" Display="'Index 90°'" Dmx="21~21" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="4" Name="Index 90-180" Display="'Index %.f°',91~179" Dmx="22~41" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="5" Name="Index 180°" Display="'Index 180°'" Dmx="42~42" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="6" Name="Index 180-270" Display="'Index %.f°',181~269" Dmx="43~62" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="7" Name="Index 270°" Display="'Index 270°'" Dmx="63~63" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="8" Name="Index 270-360" Display="'Index %.f°',271~359" Dmx="64~83" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="9" Name="Index 360°" Display="'Index 360°'" Dmx="84~84" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="10" Name="Index 360-450" Display="'Index %.f°',361~449" Dmx="85~104" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="11" Name="Index 450°" Display="'Index 450°'" Dmx="105~105" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="12" Name="Index 450-540" Display="'Index %.f°',451~539" Dmx="106~126" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="13" Name="Index 540°" Display="'Index 540°'" Dmx="127~127" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="14" Name="CW" Display="'CW %.2f RPM',120.00~0.00" Dmx="128~190" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="15" Name="Stopped" Display="'Stopped'" Dmx="191~192" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="16" Name="CCW" Display="'CCW %.2f RPM',0.00~120.00" Dmx="193~255" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Gobo1" Name="Gobo 1" Description="" Group="G">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~0" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="Gobo 1" Display="'Gobo 1'" Dmx="37~73" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="Gobo 2" Display="'Gobo 2'" Dmx="74~110" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="4" Name="Gobo 3" Display="'Gobo 3'" Dmx="111~147" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="5" Name="Gobo 4" Display="'Gobo 4'" Dmx="148~184" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="6" Name="Gobo 5" Display="'Gobo 5'" Dmx="185~221" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="7" Name="Gobo 6" Display="'Gobo 6'" Dmx="222~255" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Dimmer" Name="Dimmer" Description="" Group="I">
      <Locate Locate="1:100" PowerOn="1:0" />
      <Condition Name="No Strobe" ID="1" If="'DimmerFunc'==1" />
      <Condition Name="Strobe Enabled" ID="2,3,4,5" If="'DimmerFunc'==2" />
      <Else ID="1,2,3,4,5" />
      <Function ID="1" Name="Dimmer" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Intensity="0~0" />
      <Function ID="2" Name="Dimmer" Display="'Dimmer %.1f%%',0.0~100.0" Dmx="0~128" Intensity="0~0" />
      <Function ID="3" Name="Open" Display="'Open'" Dmx="129~139" Intensity="0~0" />
      <Function ID="4" Name="Strobe" Display="'Strobe %.1f Hz',1.0~7.0" Dmx="140~243" Intensity="0~0" />
      <Function ID="5" Name="Open (Strobe)" Display="'Open (Strobe)'" Dmx="244~255" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Pan" Name="Pan" Description="" Group="P" Size="2">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Pan" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" />
    </Attribute>
    <Attribute ID="Tilt" Name="Tilt" Description="" Group="P" Size="2">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Tilt" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" />
    </Attribute>
    <Attribute ID="Effect" Name="Effect" Description="" Group="E">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~0" />
      <Function ID="2" Name="Warm Filter" Display="'Warm Filter'" Dmx="37~37" />
      <Function ID="3" Name="Cold Filter" Display="'Cold Filter'" Dmx="75~75" />
      <Function ID="4" Name="Light Frost" Display="'Light Frost'" Dmx="112~112" />
      <Function ID="5" Name="Heavy Frost" Display="'Heavy Frost'" Dmx="147~147" />
      <Function ID="6" Name="3 Facet Prism" Display="'3 Facet Prism'" Dmx="184~184" />
      <Function ID="7" Name="UV" Display="'UV'" Dmx="219~219" />
      <Function ID="8" Name="Lt Blue" Display="'Lt Blue'" Dmx="255~255" />
    </Attribute>
    <Attribute ID="Focus" Name="Focus" Description="" Group="B">
      <Locate Locate="1:50" PowerOn="1:0" />
      <Function ID="1" Name="Focus" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Iris="0~0" Zoom="0~0" />
    </Attribute>
    <Attribute ID="DimmerFunc" Name="Dimmer Func" Description="" Group="I" Size="0">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="No Strobe" Display="'No Strobe'" Dmx="0~0" Update="Dimmer" Intensity="0~0" />
      <Function ID="2" Name="Strobe Enabled" Display="'Strobe Enabled'" Dmx="0~0" Update="Dimmer" Intensity="0~0" />
    </Attribute>
  </Control>
  <Mode Name="8 DMX, 8 Bit" Channels="8">
    <Import PearlRef="CPCol300" DiamondRef="CPStage300" WysiwygRef="" />
    <Physical>
      <Bulb Type="HMI 1200 lamp" Lumens="110000" ColourTemp="" />
      <Lens Name="" Degrees="" />
      <Weight Kg="43.9" />
      <Size Height="0.225" Width="0.435" Depth="1.230" />
      <Focus Type="Head" PanMax="450" TiltMax="252" PanMaxSpeed="0" TiltMaxSpeed="0" />
    </Physical>
    <Include>
      <Attribute ID="Dimmer" ChannelOffset="4" Wheel="1" />
      <Attribute ID="DimmerFunc" Wheel="4" />
      <Attribute ID="Pan" ChannelOffset="5" Wheel="2" />
      <Attribute ID="Tilt" ChannelOffset="6" Wheel="3" />
      <Attribute ID="Colour1" ChannelOffset="1" Wheel="7" />
      <Attribute ID="Gobo1Rot" ChannelOffset="2" Wheel="14" />
      <Attribute ID="Gobo1" ChannelOffset="3" Wheel="13" />
      <Attribute ID="Focus" ChannelOffset="8" Wheel="19" />
