    </Include>
  </Mode>
  <Mode Name="10 DMX, 16 Bit" Channels="10">
    <Import PearlRef="CPCol300Hi" DiamondRef="CPStage300H" WysiwygRef="" />
    <Physical>
      <Bulb Type="HMI 1200 lamp" Lumens="110000" ColourTemp="" />
      <Lens Name="" Degrees="" />
      <Weight Kg="43.9" />
      <Size Height="0.225" Width="0.435" Depth="1.230" />
      <Focus Type="Head" PanMax="450" TiltMax="252" PanMaxSpeed="0" TiltMaxSpeed="0" />
    </Physical>
    <Include>
      <Attribute ID="Dimmer" ChannelOffset="4" Wheel="1" />
      <Attribute ID="DimmerFunc" Wheel="4" />
      <Attribute ID="Pan" ChannelOffset="5,9" Wheel="2" />
      <Attribute ID="Tilt" ChannelOffset="6,10" Wheel="3" />
      <Attribute ID="Colour1" ChannelOffset="1" Wheel="7" />
      <Attribute ID="Gobo1Rot" ChannelOffset="2" Wheel="14" />
      <Attribute ID="Gobo1" ChannelOffset="3" Wheel="13" />
      <Attribute ID="Focus" ChannelOffset="8" Wheel="19" />
      <Attribute ID="Effect" ChannelOffset="7" Wheel="22" />
    </Include>
  </Mode>
  <Macro>
    <Macro Name="Reset">
      <Set Channel="Colour1:1:0,Gobo1Rot:1:0,Gobo1:1:0,Dimmer:1:0,Pan:1:0,Tilt:1:0,Effect:1:0,Focus:1:0,DimmerFunc:1" Delay="6000" />
      <Set Channel="Colour1:1:0,Gobo1Rot:1:0,Gobo1:1:0,Dimmer:1:255,Pan:1:50,Tilt:1:50,Effect:1:0,Focus:1:50,DimmerFunc:1" Delay="0" />
    </Macro>
  </Macro>
  <Palettes>
    <Group Name="Colour">
      <Palette ID="1" Name="Open" Number="1" Page="1" SetControl="Colour1:1" />
      <Palette ID="2" Name="Red" Number="2" Page="1" SetControl="Colour1:3" />
      <Palette ID="3" Name="Orange" Number="3" Page="1" SetControl="Colour1:11" />
      <Palette ID="4" Name="Yellow" Number="4" Page="1" SetControl="Colour1:5" />
      <Palette ID="5" Name="Yellow" Number="0" Page="1" SetControl="Colour1:5" />
      <Palette ID="6" Name="Green" Number="5" Page="1" SetControl="Colour1:9" />
      <Palette ID="7" Name="Green" Number="0" Page="1" SetControl="Colour1:9" />
      <Palette ID="8" Name="Open" Number="6" Page="1" SetControl="Colour1:1" />
      <Palette ID="9" Name="Blue" Number="0" Page="1" SetControl="Colour1:13" />
      <Palette ID="10" Name="Blue" Number="7" Page="1" SetControl="Colour1:13" />
      <Palette ID="11" Name="Violet" Number="9" Page="1" SetControl="Colour1:7" />
      <Palette ID="12" Name="Pink" Number="10" Page="1" SetControl="Colour1:15" />
      <Palette ID="13" Name="Violet" Number="0" Page="1" SetControl="Colour1:7" />
      <Palette ID="14" Name="Pink" Number="8" Page="1" SetControl="Colour1:15" />
      <Palette ID="15" Name="Blue" Number="0" Page="1" SetControl="Colour1:13" />
