7     1   s   24   100   1N   K   1   "Gobo 1 Rot"        0     0     0
0     0   I   32   100   1N   K   1   "Gobo 1 R Low"      0     0     0
6    61   L   25   100   1N   J   1   "Gobo 2"            0     0     0
7    61   s   26   100   1N   M   1   "Gobo 2 Rot"        0     0     0
0     0   I   33   100   1N   M   1   "Gobo 2 R Low"      0     0     0
