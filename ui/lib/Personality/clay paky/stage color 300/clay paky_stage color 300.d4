  <Control>
    <Attribute ID="Cyan" Name="Cyan" Description="" Group="C">
      <Locate Locate="1:0" PowerOn="1:0" Highlight="1:0" Lowlight="1:0" />
      <Function ID="1" Name="Cyan C-Mix" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Colour="0,255,255" />
    </Attribute>
    <Attribute ID="Magenta" Name="Magenta" Description="" Group="C">
      <Locate Locate="1:0" PowerOn="1:0" Highlight="1:0" Lowlight="1:0" />
      <Function ID="1" Name="Magenta C-Mix" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Colour="255,0,255" />
    </Attribute>
    <Attribute ID="Yellow" Name="Yellow" Description="" Group="C">
      <Locate Locate="1:0" PowerOn="1:0" Highlight="1:0" Lowlight="1:0" />
      <Function ID="1" Name="Yellow C-Mix" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Colour="255,255,0" />
    </Attribute>
    <Attribute ID="Shutter" Name="Shutter" Description="" Group="I">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="231~255" Intensity="0~0" />
      <Function ID="2" Name="Closed" Display="'Closed'" Dmx="0~0" Intensity="0~0" />
      <Function ID="3" Name="Strobe" Display="'Strobe %.f%%',0~100" Dmx="26~230" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Pan" Name="Pan" Description="" Group="P" Size="2">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Pan" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" />
    </Attribute>
    <Attribute ID="Tilt" Name="Tilt" Description="" Group="P" Size="2">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Tilt" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" />
    </Attribute>
    <Attribute ID="Dimmer" Name="Dimmer" Description="" Group="I">
      <Locate Locate="1:100" PowerOn="1:0" />
      <Function ID="1" Name="Dimmer" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Frost" Name="Frost" Description="" Group="B">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="Frost" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Iris="0~0" Zoom="0~0" />
    </Attribute>
  </Control>
  <Mode Name="8 DMX, 8 Bit" Channels="8">
    <Import PearlRef="CPCol300" DiamondRef="CPCol300" WysiwygRef="" />
    <Physical>
      <Bulb Type="HMI 1200 lamp" Lumens="110000" ColourTemp="" />
      <Lens Name="" Degrees="" />
      <Weight Kg="43.9" />
      <Size Height="0.225" Width="0.435" Depth="1.230" />
      <Focus Type="Head" PanMax="450" TiltMax="252" PanMaxSpeed="0" TiltMaxSpeed="0" />
    </Physical>
    <Include>
      <Attribute ID="Shutter" ChannelOffset="4" Wheel="4" />
      <Attribute ID="Dimmer" ChannelOffset="7" Wheel="1" />
      <Attribute ID="Pan" ChannelOffset="5" Wheel="2" />
      <Attribute ID="Tilt" ChannelOffset="6" Wheel="3" />
      <Attribute ID="Cyan" ChannelOffset="1" Wheel="10" />
      <Attribute ID="Magenta" ChannelOffset="2" Wheel="11" />
      <Attribute ID="Yellow" ChannelOffset="3" Wheel="12" />
      <Attribute ID="Frost" ChannelOffset="8" Wheel="19" />
    </Include>
  </Mode>
