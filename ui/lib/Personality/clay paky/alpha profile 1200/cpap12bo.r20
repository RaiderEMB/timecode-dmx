2     1   L    8   100   1N   G   1   "Iris"            131     0     0
6     1   I    9   100   1N   I   1   "Gobo 1"            0     0     0
6    61   I   10   100   1N   J   1   "Gobo 2"            0     0     0
7    61   s   11   100   1N   M   1   "Gobo 2 Rot"        0     0     0
0     0   L   31   100   1N   M   1   "Gobo 2 R Low"      0     0     0
