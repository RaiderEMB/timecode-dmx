Device CP_AW12Std2

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 7
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 3 0 0
    Mapping 4 103 30 300
    Mapping 104 107 1 1
    Mapping 108 207 100 200
    Mapping 208 212 1 1
    Mapping 213 251 150 150
    Mapping 252 255 1 1
End

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 11
    Mapping 0 255 -265.000000 265.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 12
    Mapping 0 255 -135.000000 135.000000
End

// Cyan
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 255 0
End

// Magenta
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 255 0
End

// Yellow
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 255 0
End

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 50 White
    Mapping 51 101 RGB220,220,240
    Mapping 102 152 RGB240,0,0
    Mapping 153 203 RGB0,0,240
    Mapping 204 255 RGB180,0,240
End

// Zoom
ChanRef Zoom
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 10
    Mapping 0 255 10 30
End

