      <Function ID="4" Name="Network Mode ON" Display="'Network Mode ON'" Dmx="150~150" Update="Sequence, Speed, Step, Red, Green, Blue" />
      <Function ID="5" Name="Safe" Display="'Safe'" Dmx="0~0" />
