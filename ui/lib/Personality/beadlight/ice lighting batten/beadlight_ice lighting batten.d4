      <Function ID="1" Name="No Func" Display="'No Func'" Dmx="0~0" />
      <Function ID="3" Name="Frame 2" Display="'Frame 2'" Dmx="128~255" />
    </Attribute>
    <Attribute ID="Red" Name="Red" Description="" Group="C">
