Device EXLEDKLS400

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 9 1 1
    Mapping 10 255 30 300
End

// Red 1
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Green 1
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

// Blue 1
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 255 0 255
End

