PFTABLE "Positions" -1  1  2
"Middle"    21  80  80
"Left"      22  50  80
"Right"     23  b0  80
"Top"       24  80  50
"Bottom"    25  80  b0
"TLeft"     26  50  50
"TRight"    27  b0  b0
"BLeft"     28  50  b0
"BRight"    29  b0  50
PFEND
;
; A color table for the colour wheel
PFTABLE "Color" -1   4
"Open"           1   0
"LightBlue"      2  14
"Red"            3  23
"Blue"           4  32
"Green"          5  41
"Yellow"         6  4b
"Magenta"        7  5a
"Lt Green"       8  69
"Dk Green"       9  78
PFEND

;
PFTABLE "Gobos"  30   5
"Open"           11   0
"Gobo  1"        12  14
"Gobo  2"        13  23
"Gobo  3"        14  32
"Gobo  4"        15  41
"Gobo  5"        16  4b
"Gobo  6"        17  5a
"Gobo  7"        18  69
PFEND
;
; Macros
MACRO "LAMP ON" 1   12
"data"          0   46
"delay"         1   50
"data"          0   0
MEND
;
MACRO "LAMP OFF" 2   12
"data"           0   e6
"delay"          1   50
"data"           0    0
MEND
;
MACRO "RESET" 3   12
"data"           0   57
"delay"          1   50
"data"           0    0
MEND
;
MACRO "CLEAR" 4   10   11
"data"        0   2D   0
MEND