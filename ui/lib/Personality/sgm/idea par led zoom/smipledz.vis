Device SM_IParLEDZ

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 31 0 0
    Mapping 32 63 1 1
    Mapping 64 95 30 300
    Mapping 96 127 1 1
    Mapping 160 191 1 1
    Mapping 192 223 30 300
    Mapping 224 255 1 1
End

// Shutter
ChanRef Dimmer
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 128 159 10 100		//Pulse
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 7
    Mapping 0 255 0 255
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Colour Macros
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 White
End

// Zoom
ChanRef Zoom
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 8
    Mapping 0 255 10 30
End

