; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 4  ;Shutter
0   31   "Closed"
32  63   "Dim %3d%%"0+(((c-a)*100)/(b-a))
64  95   "Strobe %3d%%"0+(((c-a)*100)/(b-a))
96  127  "Open"
128 159  "Pulse %3d%%"0+(((c-a)*100)/(b-a))
160 191  "Open"
192 223  "Rnd St %3d%%"0+(((c-a)*100)/(b-a))
224 255  "Open"
REND

RANGE 1  ;Colour 1
0   24   "Open"
25  49   "Colour  1"
50  74   "Colour  2"
75  99   "Colour  3"
100 124  "Colour  4"
125 149  "Colour  5"
150 174  "Colour  6"
175 199  "Colour  7"
200 255  "Scroll %3d%%"0+(((c-a)*100)/(b-a))
REND

RANGE 7  ;P/T Speed
0   225  "Speed %3d%%"100-(((c-a)*100)/(b-a))
226 235  "BO P/T"
236 245  "BO All"
