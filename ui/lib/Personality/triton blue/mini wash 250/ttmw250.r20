REND

RANGE 8  ;Control
0   19   "Normal Col"
20  39   "Any Col"
40  59   "Lamp On"
60  79   "Lamp Off"
80  99   "Reset All"
100 119  "Macro 1"
120 139  "Macro 2"
140 159  "Macro 3"
160 179  "Macro 4"
180 199  "Macro 5"
200 219  "Macro 6"
220 239  "Macro 7"
240 255  "Macro 8"
REND

