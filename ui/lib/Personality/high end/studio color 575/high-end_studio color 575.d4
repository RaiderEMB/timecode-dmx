      <Function ID="19" Name="No Scroll" Display="'No Scroll'" Dmx="0~3" Colour="0,0,0" />
      <Function ID="20" Name="Scroll CW" Display="'Scroll CW %.f%%',0~100" Dmx="4~127" Colour="0,0,0" />
