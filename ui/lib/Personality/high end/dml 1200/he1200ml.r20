"data"                0  28
"delay"               1  32
"data"                0  10
MEND

MACRO "Locate no P/T"  2  15
"data"                0  38
"delay"               1  32
"data"                0  10
MEND

MACRO "Locate P/T"    3  15
"data"                0  48
"delay"               1  32
"data"                0  10
MEND

MACRO "Locate CMY"    4  15
"data"                0  58
"delay"               1  32
"data"                0  10
MEND

MACRO "Lamp On"       5  15
"data"                0  70
"delay"               1  32
"data"                0  10
MEND

MACRO "Lamp Off"      6  15
