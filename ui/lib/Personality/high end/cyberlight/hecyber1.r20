; Positions
PFTABLE "Positions" -1  1   3
"Middle"      21 80 80
"Left"       22 50 80
"Right"       23 b0 80
"Top"        24 80 50
"Bottom"      25 80 b0
"TLeft"       26 50 50
"TRight"      27 b0 b0
"BLeft"       28 50 b0
"BRight"      29 b0 50
PFEND

MACRO "HOME" 1  18  20
"data"    0 ff ff 
"delay"   1  3
"data"    0  0  ff 
"delay"   1  3
"data"    0  0 40
"delay"   1 40
"data"    0  0  0
MEND

MACRO "SHUTDOWN" 2 18 20
"data"      0 ff ff 
"delay"     1  3
"data"      0  0 ff 
"delay"     1  3
"data"      0  0 80
"delay"     1 40
"data"      0  0  0
MEND


;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 17 ;Shutter
0  0  "Closed"
4  247 "Strobe %3d%%"0+(((c-a)*100)/(b-a))
255 255 "Open"
REND

RANGE 6 ;Cyan
0  255 "%3d%%"100-(((c-a)*100)/(b-a))
REND

RANGE 7 ;Magenta
0  255 "%3d%%"100-(((c-a)*100)/(b-a))
REND

RANGE 8 ;Yellow
0  255 "%3d%%"100-(((c-a)*100)/(b-a))
REND

RANGE 16 ;Frost
255 255 "No Frost"
0 254  "%3d%% "100-(((c-a)*100)/(b-a))
REND

RANGE 15 ;Effect
44 44  "Prism"
0  255 "%3d "0+(((c-a)*255)/(b-a))
REND

RANGE 20 ;Control
0  63  "Safe"
65 127  "Safe"
129 255  "safe"
64 64  "Home"
128 128  "Shutdown"
REND

