"Red"            2  7F  DD  F9
"Amber"          3  4C  FD  C8
"Yellow"         4  00  E6  23
"Green"          5  DB  D1  0B
"Cyan"           6  DC  83  0A
"Blue"           7  DE  08  D3
"Lavender"       8  B6  03  E3
"UltraViolet"    9  FF  79  FB
"Magenta"       10  79  93  FF
PFEND

;----------------------------------------------------------------------------
; Macros

MACRO "Reset"         1   8
"data"                0  5A
"delay"               1  50
"data"                0   A
"delay"               1   A
MEND

;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 1  ;Colour 1
0   21   "Closed"
22  43   "Open"
44  65   "Red"
66  87   "Blue"
88  109  "Green"
110 127  "5600K"
128 187  "Sc CW %3d%%"100-(((c-a)*100)/(b-a))
188 193  "No Scroll"
194 255  "Sc CCW %3d%%"0+(((c-a)*100)/(b-a))
REND

RANGE 8  ;Control
0   19   "Col Normal"
20  39   "Col Indexin"
40  79   "Safe"
80  99   "Reset"
100 119  "Macro 1"
120 139  "Macro 2"
140 159  "Macro 3"
160 179  "Macro 4"
180 199  "Macro 5"
200 219  "Macro 6"
