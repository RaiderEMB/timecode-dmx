;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 1  ;Control
0   9    "Safe"
10  16   "Display"
17  19   "Safe"
20  26   "DMX Display"
27  29   "Safe"
30  36   "Menu DisplY"
37  49   "Safe"
50  56   "Reset All"
57  219  "Safe"
220 226  "BO w P/T"
227 255  "Safe"
REND

RANGE 3  ;Shutter
0   9    "Closed"
10  19   "Open"
20  65   "Strobe %3d%%"0+(((c-a)*100)/(b-a))
66  75   "Open"
76  107  "Cl Pul %3d%%"0+(((c-a)*100)/(b-a))
108 139  "Op Pul %3d%%"0+(((c-a)*100)/(b-a))
140 149  "Open"
150 159  "R Strobe F"
160 169  "R Strobe M"
170 179  "R Strobe S"
180 189  "Open"
190 199  "R Op Pul F"
200 209  "R Op Pul M"
210 219  "R Op Pul S"
220 229  "R Cl Pul F"
230 239  "R Cl Pul M"
240 249  "R Cl Pul S"
250 255  "Open"
REND

RANGE 11  ;Macro
0   5    "Safe"
6   255  "Macro %3d%%"0+(((c-a)*100)/(b-a))
REND

RANGE 12  ;P/T Speed
0   9    "Tracking"
