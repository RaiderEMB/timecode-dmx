Device HQVDP250SC6

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 0 1 1
    Mapping 1 255 30 300
End

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 -180.000000 180.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 -90.000000 90.000000
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 255 0 255
End

