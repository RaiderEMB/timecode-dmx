    <Include>
      <Attribute ID="Shutter" ChannelOffset="3" Wheel="4" />
      <Attribute ID="Pan" ChannelOffset="5" Wheel="2" />
      <Attribute ID="Tilt" ChannelOffset="6" Wheel="3" />
      <Attribute ID="Colour1" ChannelOffset="1" Wheel="8" />
