    <Include>
      <Attribute ID="Dimmer" ChannelOffset="5" Wheel="1" />
      <Attribute ID="Colour1Func" Wheel="7" />
      <Attribute ID="Colour1" ChannelOffset="6" Wheel="8" />
      <Attribute ID="Cyan" ChannelOffset="2" Wheel="10" />
