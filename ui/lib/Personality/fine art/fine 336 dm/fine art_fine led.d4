    <Attribute ID="PT_Speed" Name="P/T Speed" Description="" Group="S">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Tracking" Display="'Tracking'" Dmx="0~2" />
      <Function ID="2" Name="Speed" Display="'Speed %.1f%%',0.0~100.0" Dmx="3~239" />
      <Function ID="3" Name="Slow" Display="'Slow'" Dmx="240~242" />
      <Function ID="4" Name="Medium" Display="'Medium'" Dmx="243~245" />
      <Function ID="5" Name="High" Display="'High'" Dmx="246~248" />
      <Function ID="6" Name="Blackout" Display="'Blackout'" Dmx="249~255" />
    </Attribute>
    <Attribute ID="Shutter" Name="Shutter" Description="" Group="I">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~0" Intensity="0~0" />
      <Function ID="2" Name="Strobe %" Display="'Strobe %.1f%%',0.0~100.0" Dmx="1~127" Intensity="0~0" />
      <Function ID="3" Name="Open Pulse" Display="'Open Pulse %.f%%',0~100" Dmx="128~159" Intensity="0~0" />
      <Function ID="4" Name="Close Pulse" Display="'Close Pulse %.f%%',0~100" Dmx="160~191" Intensity="0~0" />
      <Function ID="5" Name="Rnd Strobe" Display="'Rnd Strobe %.f%%',0~100" Dmx="192~255" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Control" Name="Control" Description="" Group="S">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Safe" Display="'Safe'" Dmx="0~7" />
      <Function ID="2" Name="Reset" Display="'Reset'" Dmx="8~14" />
      <Function ID="3" Name="Safe" Display="'Safe'" Dmx="15~255" />
    </Attribute>
    <Attribute ID="Dimmer" Name="Dimmer" Description="" Group="I">
      <Locate Locate="1:100" PowerOn="1:0" />
      <Function ID="1" Name="Dimmer" Display="'%.1f%%',0.0~100.0" Dmx="255~0" Intensity="0~0" />
    </Attribute>
