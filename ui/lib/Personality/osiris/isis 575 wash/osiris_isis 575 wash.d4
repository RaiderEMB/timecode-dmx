  <Macro>
    <Macro Name="Lamp On">
      <Set Channel="Iris:6" Delay="26000" />
      <Set Channel="Iris:1" Delay="1000" />
    </Macro>
    <Macro Name="Lamp Off">
      <Set Channel="Iris:4" Delay="26000" />
      <Set Channel="Iris:1" Delay="1000" />
    </Macro>
    <Macro Name="Reset">
      <Set Channel="Iris:2" Delay="26000" />
      <Set Channel="Iris:1" Delay="1000" />
    </Macro>
  </Macro>
