    <Include>
      <Attribute ID="Dimmer" ChannelOffset="3" Wheel="1" />
      <Attribute ID="Shutter" ChannelOffset="4" Wheel="4" />
      <Attribute ID="Pan" ChannelOffset="5,7" Wheel="2" />
