Device EL_DL36MH10

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 9
    Mapping 0 0 1 1
    Mapping 1 95 30 300
    Mapping 96 127 1 1
    Mapping 128 159 100 200
    Mapping 160 191 1 1
    Mapping 192 233 150 150
    Mapping 224 255 1 1
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 10
    Mapping 0 255 0 255
End

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 -270.000000 270.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 -132.000000 132.000000
End

// Colour Macros
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 7 White
End

// Red
ChanRef Red
