Device JB_LEDPWRBM

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 9 1 1
    Mapping 10 255 30 300
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Col Mac
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 9 White
    Mapping 10 29 Red
    Mapping 30 39 RGB240,40,0
    Mapping 40 49 RGB240,100,0
    Mapping 50 69 Yellow
    Mapping 70 79 RGB200,240,0
    Mapping 80 89 RGB140,240,0
    Mapping 90 109 Green
    Mapping 110 119 RGB0,200,40
    Mapping 120 129 RGB0,140,100
    Mapping 130 149 Blue
    Mapping 150 159 RGB0,40,240
    Mapping 160 169 RGB0,100,240
    Mapping 170 189 Cyan
    Mapping 190 199 RGB100,180,240
    Mapping 200 219 Purple
    Mapping 220 229 RGB100,50,120
    Mapping 230 239 Warm white
    Mapping 240 255 White
End
 
