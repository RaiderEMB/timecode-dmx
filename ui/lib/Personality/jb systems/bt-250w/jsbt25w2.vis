Device JS_BT250W8

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 7 0 0
    Mapping 8 15 1 1
    Mapping 16 239 30 300
    Mapping 240 247 150 150
    Mapping 248 255 1 1
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 7
    Mapping 0 255 0 255
End

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 -270.000000 270.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 -135.000000 135.000000
End

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 18 White
    Mapping 19 36 RGB240,0,0
    Mapping 37 54 RGB240,240,0
    Mapping 55 72 RGB240,120,0
    Mapping 73 90 RGB0,240,0
    Mapping 91 108 RGB0,120,240
    Mapping 109 127 RGB120,0,240
End

// Colour
ChanRef Color Wheel 1
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 128 247 5 50
    Mapping 248 255 30 30
End

