Device JB_LEDOcPn2

// Shutter
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 255 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 15 1 1
    Mapping 16 255 30 300
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 255 0 255
End

