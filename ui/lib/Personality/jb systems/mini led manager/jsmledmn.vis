Device JS_mLEDMan

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 7 0 0
    Mapping 8 190 0 255
    Mapping 191 255 255 255
    Mapping 201 247 0 0
    Mapping 248 255 0 0
End

// Dimmer
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 7 0 0
    Mapping 8 190 1 1
    Mapping 191 200 150 150
    Mapping 201 247 30 300
    Mapping 248 255 1 1
End

