    <Attribute ID="Speed" Name="Speed" Description="" Group="S">
      <Locate Locate="1:0" PowerOn="1:0" />
      <Function ID="1" Name="Tracking" Display="'Tracking'" Dmx="0~4" />
      <Function ID="2" Name="Generic Range" Display="'%.f%%',100~0" Dmx="255~5" />
