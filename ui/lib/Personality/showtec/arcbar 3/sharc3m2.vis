Device SH_ARC3M2

// Shutter
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 8
    Mapping 0 255 255 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 8
    Mapping 0 4 1 1
    Mapping 5 255 30 300
End

// Module 1
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 5 White
    Mapping 5 34 RGB240,0,0
    Mapping 35 64 RGB0,240,0
    Mapping 65 94 RGB0,0,240
    Mapping 95 124 RGB240,240,0
    Mapping 125 154 RGB0,240,240
    Mapping 155 184 RGB120,0,240
    Mapping 185 255 RGB240,140,140
End

