Device SH_LMSBRGB3

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

