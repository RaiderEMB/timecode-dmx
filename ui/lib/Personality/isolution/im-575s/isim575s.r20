"delay"               1  46
"data"                0  00 00
MEND

MACRO "Lamp Off"      2  16 8
"data"                0  FF EF
"delay"               1  46
"data"                0  00 00
MEND

MACRO "Reset"         3  16 8
"data"                0  FF FF
"delay"               1  46
"data"                0  00 00
MEND

