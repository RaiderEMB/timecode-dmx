Device ISO_SW-90M3

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 7
    Mapping 0 7 0 0
    Mapping 8 15 1 1
    Mapping 16 131 30 300
    Mapping 132 139 1 1
    Mapping 182 189 1 1
    Mapping 232 239 1 1
    Mapping 240 247 150 150
    Mapping 248 255 1 1
End

// Shutter
ChanRef Dimmer
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 7
	Mapping 140 181 10 100		//Open Pulse	
    Mapping 190 231 -10 -100		//Close Pulse
End

	