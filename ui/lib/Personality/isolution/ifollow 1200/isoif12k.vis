Device ISO_IFOL12K

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 7 0 0
    Mapping 8 15 1 1
    Mapping 16 247 30 300
    Mapping 248 255 1 1
End

// Iris
ChanRef Iris
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 1
End

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 14 White
    Mapping 0 14 White
    Mapping 0 14 White
    Mapping 15 28 RGB240,0,0
    Mapping 29 42 RGB240,240,0
    Mapping 43 56 RGB240,0,240
    Mapping 57 70 RGB0,240,0
    Mapping 71 84 RGB240,120,0
    Mapping 85 98 RGB0,0,240
    Mapping 99 112 RGB180,0,240
    Mapping 113 127 RGB240,140,140
    Mapping 128 191 Black
    Mapping 192 255 Black
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

