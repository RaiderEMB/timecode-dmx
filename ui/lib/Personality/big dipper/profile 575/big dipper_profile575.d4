  <Manual Filename="" Summary="" />
  <Control>
    <Attribute ID="Pan" Name="Pan" Description="" Group="P" Size="2">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Pan" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" />
    </Attribute>
    <Attribute ID="Tilt" Name="Tilt" Description="" Group="P" Size="2">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Tilt" Display="'%.2f%%',0.00~100.00" Dmx="0~65535" />
    </Attribute>
    <Attribute ID="Colour1Func" Name="Colour Func" Description="" Group="C" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Fixed" Display="'Fixed'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="2" Name="Scroll" Display="'Scroll'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
      <Function ID="3" Name="Raw" Display="'Raw'" Dmx="0~0" Update="Colour1" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Colour1" Name="Colour" Description="" Group="C">
      <Locate Locate="1" PowerOn="1" Highlight="8" Lowlight="5" />
      <Condition Name="Fixed" ID="1,2,3,4,5,6,7,8" If="'Colour1Func'==1" />
      <Condition Name="Scroll" ID="9,10" If="'Colour1Func'==2" />
      <Else ID="11" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~15" Colour="255,255,255" />
      <Function ID="2" Name="Red" Display="'Red'" Dmx="16~31" Colour="255,0,0" />
      <Function ID="3" Name="Green" Display="'Green'" Dmx="32~47" Colour="0,255,0" />
      <Function ID="4" Name="Pink" Display="'Pink'" Dmx="48~63" Colour="255,150,150" />
      <Function ID="5" Name="Blue" Display="'Blue'" Dmx="64~79" Colour="0,0,255" />
      <Function ID="6" Name="Orange" Display="'Orange'" Dmx="80~95" Colour="255,128,0" />
      <Function ID="7" Name="Cyan" Display="'Cyan'" Dmx="96~111" Colour="0,255,255" />
      <Function ID="8" Name="Yellow" Display="'Yellow'" Dmx="112~127" Colour="255,255,0" />
      <Function ID="9" Name="No Scroll" Display="'No Scroll'" Dmx="0~0" Colour="0,0,0" />
      <Function ID="10" Name="Scroll CW" Display="'Scroll CW %.1f%%',100.0~0.0" Dmx="255~128" Colour="0,0,0" />
      <Function ID="11" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" Colour="0,0,0" />
    </Attribute>
    <Attribute ID="Gobo1Func" Name="Gobo Func" Description="" Group="G" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Spin" Display="'Spin'" Dmx="0~0" Update="Gobo1, Gobo1Rot" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="Scroll" Display="'Scroll'" Dmx="0~0" Update="Gobo1, Gobo1Rot" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="Raw" Display="'Raw'" Dmx="0~0" Update="Gobo1, Gobo1Rot" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Gobo1" Name="Gobo" Description="" Group="G">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Spin" ID="1,2,3,4,5,6,7,8" If="'Gobo1Func'==1" />
      <Condition Name="Scroll" ID="9,10" If="'Gobo1Func'==2" />
      <Else ID="11" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~15" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="Gobo 1" Display="'Gobo 1'" Dmx="16~31" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="Gobo 2" Display="'Gobo 2'" Dmx="32~47" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="4" Name="Gobo 3" Display="'Gobo 3'" Dmx="48~63" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="5" Name="Gobo 4" Display="'Gobo 4'" Dmx="64~79" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="6" Name="Gobo 5" Display="'Gobo 5'" Dmx="80~95" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="7" Name="Gobo 6" Display="'Gobo 6'" Dmx="96~111" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="8" Name="Gobo 7" Display="'Gobo 7'" Dmx="112~127" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="9" Name="No Scroll" Display="'No Scroll'" Dmx="0~0" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="10" Name="Scroll CW" Display="'Scroll CW %.1f%%',100.0~0.0" Dmx="255~128" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="11" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Gobo1Rot" Name="Gobo Rot" Description="" Group="G">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Spin" ID="1,2,3,4,5,6" If="'Gobo1Func'&lt;3" />
      <Else ID="7" />
      <Function ID="1" Name="No Spin" Display="'No Spin'" Dmx="179~171" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="Spin CW" Display="'Spin CW %.f%%',100~0" Dmx="170~88" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="No Spin" Display="'No Spin'" Dmx="87~84" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="4" Name="Spin CCW" Display="'Spin CCW %.f%%',0~100" Dmx="83~1" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="5" Name="No Spin" Display="'No Spin'" Dmx="0~0" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="6" Name="Random" Display="'Random %.f%%',0~100" Dmx="180~255" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="7" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" GoboSpin="0~0" GoboOrientation="0~0" />
    </Attribute>
    <Attribute ID="Dimmer" Name="Dimmer" Description="" Group="I">
      <Locate Locate="1:100" PowerOn="1:0" />
      <Function ID="1" Name="Dimmer" Display="'%.1f%%',0.0~100.0" Dmx="0~130" Intensity="0~0" />
      <Function ID="2" Name="Strobe %" Display="'Strobe %.1f%%',0.0~100.0" Dmx="131~253" Intensity="0~0" />
      <Function ID="3" Name="Open" Display="'Open'" Dmx="254~255" Intensity="0~0" />
    </Attribute>
    <Attribute ID="Prism_Func" Name="Prism Func" Description="" Group="E" Size="0">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Prism" Display="'Prism'" Dmx="0~0" Update="Prism" />
      <Function ID="2" Name="Raw" Display="'Raw'" Dmx="0~0" Update="Prism" />
    </Attribute>
    <Attribute ID="Prism" Name="Prism" Description="" Group="E">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Prism" ID="1,2,4,5,6,7,8,9" If="'Prism_Func'==1" />
      <Else ID="3" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="0~15" />
      <Function ID="2" Name="Prism" Display="'Prism'" Dmx="16~63" />
      <Function ID="3" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" />
      <Function ID="4" Name="No Spin" Display="'No Spin'" Dmx="199~192" />
      <Function ID="5" Name="Spin CW" Display="'Spin CW %.f%%',100~0" Dmx="191~127" />
      <Function ID="6" Name="No Spin" Display="'No Spin'" Dmx="192~192" />
      <Function ID="7" Name="Spin CCW" Display="'Spin CCW %.f%%',0~100" Dmx="126~64" />
      <Function ID="8" Name="No Spin" Display="'No Spin'" Dmx="192~192" />
      <Function ID="9" Name="Random" Display="'Random %.f%%',0~100" Dmx="200~255" />
    </Attribute>
    <Attribute ID="Focus" Name="Focus" Description="" Group="B">
      <Locate Locate="1:50" PowerOn="1:50" />
      <Function ID="1" Name="Focus" Display="'%.1f%%',0.0~100.0" Dmx="0~255" Iris="0~0" Zoom="0~0" />
    </Attribute>
    <Attribute ID="PT_Speed" Name="P/T Speed" Description="" Group="S">
      <Locate Locate="1:100" PowerOn="1:100" />
      <Function ID="1" Name="Speed" Display="'%.1f%%',100.0~0.0" Dmx="255~0" />
    </Attribute>
    <Attribute ID="Control" Name="Control" Description="" Group="S">
      <Locate Locate="1" PowerOn="1" />
      <Function ID="1" Name="Safe" Display="'Safe'" Dmx="0~254" />
      <Function ID="2" Name="Reset" Display="'Reset'" Dmx="255~255" />
    </Attribute>
  </Control>
