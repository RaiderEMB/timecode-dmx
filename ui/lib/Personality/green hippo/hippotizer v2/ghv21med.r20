0     0   L   11   100   1N   0   1   "Rotation Low"      0     0     0
3    61   s   12   100   1N   E   1   "X-Pos"           128     0     0
0     0   L   13   100   1N   E   1   "X-Pos Low"       128     0     0
3     1   s   14   100   1N   F   1   "Y-Pos"           128     0     0
