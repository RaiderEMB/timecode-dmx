      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed" ID="1,2,3,4,5" If="'Gobo1Func'==1" />
      <Condition Name="Index" ID="10,11,12,13,14" If="'Gobo1Func'==2" />
      <Condition Name="Scroll" ID="6,7,8,9" If="'Gobo1Func'==3" />
      <Else ID="15" />
      <Function ID="1" Name="Open" Display="'Open'" Dmx="181~185" WheelPosition="0" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="2" Name="Small" Display="'Small'" Dmx="176~180" WheelPosition="1" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="3" Name="Cone" Display="'Cone'" Dmx="171~175" WheelPosition="2" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="4" Name="Medium" Display="'Medium'" Dmx="166~170" WheelPosition="3" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="5" Name="Dots" Display="'Dots'" Dmx="161~165" WheelPosition="4" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="6" Name="No Scroll" Display="'No Scroll'" Dmx="0~0" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="7" Name="Scroll CW" Display="'Scroll CW %.f%%',100~0" Dmx="186~214" GoboSpin="0~0" GoboOrientation="0~0" />
