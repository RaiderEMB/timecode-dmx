      <Function ID="10" Name="Open/Small" Display="'Open/Small %.f%%',0~100" Dmx="0~31" WheelPosition="0" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="11" Name="Small/Cone" Display="'Small/Cone %.f%%',0~100" Dmx="32~63" WheelPosition="1" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="12" Name="Cone/Medium" Display="'Cone/Medium %.f%%',0~100" Dmx="64~95" WheelPosition="2" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="13" Name="Medium/Dots" Display="'Medium/Dots %.f%%',0~100" Dmx="96~127" WheelPosition="3" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="14" Name="Dots/Open" Display="'Dots/Open %.f%%',0~100" Dmx="128~160" WheelPosition="4" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="15" Name="Raw DMX" Display="'Raw DMX %.f',0~255" Dmx="0~255" GoboSpin="0~0" GoboOrientation="0~0" />
