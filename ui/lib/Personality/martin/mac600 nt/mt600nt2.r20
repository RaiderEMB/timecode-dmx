RANGE 1  ;Shutter
20  49   "Open"
0   19   "Closed"
50  112  "Strobe %3d%%"100-(((c-a)*100)/(b-a))
112 127   "Open"
128 187  "Rnd St %3d%%"100-(((c-a)*100)/(b-a))
188 207   "Open"
208 217  "Reset"
218 227   "Safe"
228 237  "Lamp On"
238 247   "Safe"
248 255  "Lamp Off"
REND

RANGE 6  ;Colour
181 185  "Open"
