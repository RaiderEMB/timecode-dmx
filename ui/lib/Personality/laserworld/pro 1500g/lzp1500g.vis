Device LZ_PRO1500G

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 1 1 1
    Mapping 2 255 30 300
End

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 8
    Mapping 0 255 -20.000000 20.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 9
    Mapping 0 255 -20.000000 20.000000
End

// Zoom
ChanRef Zoom
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 12
    Mapping 0 255 10 30
End


// Iris
ChanRef Iris
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0.05 0.05
End

