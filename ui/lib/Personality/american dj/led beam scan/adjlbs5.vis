Device ADJ_LEDBS5

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 8 1 1 
    Mapping 9 131 30 300
    Mapping 132 139 1 1
    Mapping 182 189 1 1
    Mapping 232 255 1 1
End

// Shutter
ChanRef Dimmer
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 140 181 10 100		//Open Pulse
    Mapping 190 231 -100 -10		//Close Pulse
End
	
// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 -265.000000 265.000000
End

