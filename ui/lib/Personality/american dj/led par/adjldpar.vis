Device ADJ_LEDPar

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 15 1.000000 1.000000
    Mapping 15 255 30.000000 300.000000
End

// Fake Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 255 255.00000 255.0000000
End

ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset  1
    Mapping 0 255 0.000000 255.000000
End

ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset  3
    Mapping 0 255 0.000000 255.000000
End

ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset  2
    Mapping 0 255 0.000000 255.000000
End
