Device SD_ACBAR150

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// White
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 White
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 1 1 1
    Mapping if 8 0 15 2 127 30 300
    Mapping if 8 16 255 128 255 30 300
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 255 0 255
End

// Rainbow
ChanRef Color Wheel 2
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 8
    Mapping 0 15 White
    Mapping 16 55 Yellow
    Mapping 56 94 Cyan
    Mapping 95 133 Green
    Mapping 134 172 Magenta    
    Mapping 173 211 Red
    Mapping 212 255 Violet Blue
End

// Rainbow
ChanRef Color Wheel 2
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 8
    Mapping 16 255 5 50
End

