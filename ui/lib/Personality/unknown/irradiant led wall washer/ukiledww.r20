;----------------------------------------------------------------------------
; Macros

;----------------------------------------------------------------------------
; Range Tables

; Column by column description...
; 1. Low DMX value (inclusive)
; 2. High DMX value (inclusive)
; 3. Displayed text (max 11 chars)
; Variables used a=low val, b=high val, c=DMX val

RANGE 1  ;Shutter
0   3    "Closed"
4   6    "Open"
7   59   "Str %2d Hz"1+((c-a)*19/(b-a))
60  69   "Op Pls %3d%%"(c-a)*100/(b-a)
70  78   "Cl Pls %3d%%"(c-a)*100/(b-a)
79  87   "Rnd St %3d%%"(c-a)*100/(b-a)
88  255  "Open"
REND

RANGE 2  ;Dimmer
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

RANGE 3  ;Red
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

RANGE 4  ;Green
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

RANGE 5  ;Blue
0   255  "%3d.%d%%"(c-a)*100/(b-a),10*((c-a)*100%(b-a))/(b-a)
REND

RANGE 6  ;CTO
0   19   "Open"
20  255  "CTO"
REND

RANGE 7  ;Macro
