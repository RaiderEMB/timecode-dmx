Device UK_LEDPAR8C

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 9 1 1
    Mapping 10 149 10 100
    Mapping 150 255 1 1
    Mapping 65535 65790 1 1
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 150 199 10 100
    Mapping 200 249 -10 -100
End

// Colour Macros
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 49  White
    Mapping 50 59 RGB240,0,0
    Mapping 60 69 RGB0,240,0
    Mapping 70 79 RGB0,0,240
    Mapping 80 89 RGB240,240,0
    Mapping 90 99 RGB0,240,240
    Mapping 100 109 RGB120,0,240
    Mapping 110 119 White
    Mapping 120 129 White
    Mapping 130 139 RGB220,220,240
    Mapping 140 149 RGB240,240,200
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 7
    Mapping 0 255 0 255
End

