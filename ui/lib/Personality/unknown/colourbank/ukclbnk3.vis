Device UK_ColBnk3

// Colour Macro
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 28 White
    Mapping 29 56 Blue
    Mapping 57 85 Green
    Mapping 86 113 Yellow
    Mapping 114 141 Cyan
    Mapping 142 170 Magenta
    Mapping 171 198 Orange
    Mapping 199 226 Purple
    Mapping 227 255 Pink
End

// Virtual Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 255 255
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 0 1 1
    Mapping 1 255 30 300
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 255 0 255
End

