PFTABLE "Color" -1   3
"White"          1   0
"Red"            2  13
"Amber"          3  5b
"Yellow"         4  3b
"Green"          5  43
"Cyan"           6  1b
"Blue"           7  53
"Lavender"       8  4b
"U Violiet"      9  63
"Magenta"       10  2b
PFEND
;
