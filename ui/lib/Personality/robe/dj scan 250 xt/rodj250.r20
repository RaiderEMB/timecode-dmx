"Left"      22  50  80
"Right"     23  b0  80
"Top"       24  80  50
"Bottom"    25  80  b0
"TLeft"     26  50  50
"TRight"    27  b0  b0
"BLeft"     28  50  b0
"BRight"    29  b0  50
PFEND
;
