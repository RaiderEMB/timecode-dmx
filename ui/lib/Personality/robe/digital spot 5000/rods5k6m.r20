"delay"               1  28
"data"                0  FF  86
"delay"               1  28
"data"                0   0  14
MEND

MACRO "Lamp Off"      2  12   8
"data"                0  FF   0
"delay"               1   A
"data"                0  FF  EA
"delay"               1  28
"data"                0  FF  14
MEND

MACRO "Shutdown"      3  12   8
"data"                0  FF   0
"delay"               1   A
"data"                0  FF  F4
"delay"               1  28
"data"                0  FF  14
MEND

MACRO "Reset"         4  12   8
"data"                0  FF   0
"delay"               1   A
"data"                0  FF  CC
"delay"               1  28
"data"                0   0  14
MEND

