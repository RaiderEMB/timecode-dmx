;   03-08-04    Phil R            Edited Lamp Off macro
;   17-09-04    Phil R            Edited Lamp On macro (renamed)
;   20-01-05    Phil Rogers       Changed naming to Robe's spec
;   26-10-05	Ed		  revised device name
;   06-09-06    D.Milligan  	  Added Range tables. Corrected max P/T. Fixed DMX Table
;
;----------------------------------------------------------------------------
; The device name, upto 11 characters, no spaces are allowed
DEVICE  RO_SCN1200                                   16 DMX channels
NAME "Robe" "Scan 1200 XT mode 1"
;
;----------------------------------------------------------------------------
; The Type of instrument, 1 & M must required for backward compatibility
; The middle number denotes the number of DMX channels used
TYPE  1  16  M
;
;----------------------------------------------------------------------------
; Mirror/Head resolution
MIRROR  1   143  114
;       |    |    |
;       |    |    |_______ maximum tilt movement (in degrees)
;       |    |____________ maximum pan movement (in degrees)
;       |_________________ 1 means moving mirror, 2 means moving head
;
;----------------------------------------------------------------------------
DEVICEADDRESS
;
DAEND
;
;----------------------------------------------------------------------------
; Specify DMX limits
;
; minimum allowed dmx number
MINDMX=1
;
; maximum allowed dmx number
MAXDMX=497
;
; DMX number must be divisible by this
DIVBY=1
;
;----------------------------------------------------------------------------
; DMX channel description
;
DMX
3   61   s   1   100   1N   E   1   "Pan"         128   -1   -1 
0    0   L   3   100   1N   E   1   "Pan_Low"     128   -1   -1 
3    1   s   2   100   1N   F   1   "Tilt"        128   -1   -1 
0    0   L   4   100   1N   F   1   "Tilt_Low"    128   -1   -1 
8    1   I   5   100   1N   0   1   "P/T Speed"     0    0    0
8   61   I   6   100   1N   0   1   "Control"       0    0    0
4    1   I   7   100   1N   A   1   "Colour1"       0    0   28 
4   61   I   8   100   1N   N   1   "Colour2"       0    0    0   
9    1   I   9   100   1N   P   1   "Prism"         0    0    0
6    1   I  10   100   1N   I   1   "Gobo1"         0    0    0
6   61   I  11   100   1N   J   1   "Gobo2"         0    0    0
7   61   L  12   100   1N   M   1   "GoboSpin"      0    0    0 
2    1   L  13   100   1N   0   1   "Iris"          0    0    0
5   61   L  14   100   1N   L   1   "Focus"        70   70   70
2   61   L  15   100   1N   O   1   "Shutter"      40   40   40
1    1   H  16   100   1N   H   1   "Dimmer"       255  255  255
END
;
;----------------------------------------------------------------------------
; Text to be displayed when the Instrument is selected for Patching
;
TEXT
  Robe Scan 1200
Bank WheelA  WheelB
---------------------
 1 Dimmer    --
 2 Iris      Shutter
 3 Tilt      Pan
 4 Colour1   Colour2
 5 --        Focus
 6 Gobo1     Gobo2
 7 --        GoboSpin
 8 Speed p/t Control
 9 Prism     --
10 --        --
11 --        --
INPUT fixture DMX No.
Press a Fixture 
Select or Swop button 
to make the patch.
END

ATTR
END
