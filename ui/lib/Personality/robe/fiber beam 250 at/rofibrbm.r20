;----------------------------------------------------------------------------
; Macros

MACRO "Lamp On"       1   3
"data"                0  86
"delay"               1  32
"data"                0  F8
"delay"               1  14
MEND

MACRO "Lamp Off"      2   3
"data"                0  EA
"delay"               1  32
"data"                0  F8
"delay"               1  14
MEND

;----------------------------------------------------------------------------
