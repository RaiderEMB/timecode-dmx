RANGE 4  ;Dimmer
0   127  "Open %3d%%"0+(((c-a)*100)/(b-a))
128 192  "Close %3d%%"0+(((c-a)*100)/(b-a))
193 255  "Frost %3d%%"0+(((c-a)*100)/(b-a))
