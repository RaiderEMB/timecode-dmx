Device SD_CCol400

// Cyan
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping if 6 0 25 0 255 255 0
    Mapping if 6 26 255 0 255 255 255
End

// Yellow
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping if 6 0 25 0 255 255 0
    Mapping if 6 26 255 0 255 255 255
End

// Magenta
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping if 6 0 25 0 255 255 0
    Mapping if 6 26 255 0 255 255 255
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 255 0 255
End

// Colour
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 25 White
    Mapping 26 51 RGB0,240,240
    Mapping 52 77 RGB240,0,240
    Mapping 78 103 RGB240,240,0
    Mapping 104 129 RGB0,0,240
    Mapping 130 155 RGB240,0,0
    Mapping 156 181 RGB0,240,0
End

// Colour
ChanRef Color Wheel 1
    Vismode Speed
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 182 255 5 50
End

