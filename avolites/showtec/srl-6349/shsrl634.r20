"Red"                 2  FF   0   0
"Amber"               3  FF  7F   0
"Yellow"              4  FF  FF   0
"Green"               5   0  FF   0
"Cyan"                6   0  FF  FF
"Blue"                7   0   0  FF
"Lavender"            8  4A  FC   5
"Purple"              9  7F   0  FF
"Magenta"            10  FF   0  FF
PFEND

;----------------------------------------------------------------------------
; Macros

;----------------------------------------------------------------------------
