Device SS_L304AStg

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 0 255
End

// Red
ChanRef Red
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 0 255
End

// Green
ChanRef Green
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 3
    Mapping 0 255 0 255
End

// Blue
ChanRef Blue
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 255 0 255
End

// Colour Macros
ChanRef Color Wheel 1
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 5
    Mapping 0 9 White
    Mapping 10 29 RGB240,0,0
    Mapping 30 39 RGB240,20,0
    Mapping 40 49 RGB240,100,0
    Mapping 50 69 RGB240,240,0
    Mapping 70 79 RGB200,240,0
    Mapping 80 89 RGB140,240,0
    Mapping 90 109 RGB0,240,0
    Mapping 110 119 RGB0,200,20
    Mapping 120 129 RGB0,140,100
    Mapping 130 149 RGB0,0,240
    Mapping 150 159 RGB0,20,240
    Mapping 160 169 RGB0,100,240
    Mapping 170 189 RGB0,240,240
    Mapping 190 199 RGB60,120,240
    Mapping 200 219 RGB120,0,240
    Mapping 220 229 RGB180,120,240
    Mapping 230 249 RGB240,240,120
    Mapping 250 255 White
End

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 6
    Mapping 0 9 1 1
    Mapping 10 255 30 300
End

