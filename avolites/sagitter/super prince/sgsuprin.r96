PFTABLE "Positions" -1   9  10
"Middle"    21 80  80
"Left"      22 50  80
"Right"     23 b0  80
"Top"       24 80  50
"Bottom"    25 80  b0
"TLeft"     26 50  50
"TRight"    27 b0  b0
"BLeft"     28 50  b0
"BRight"    29 b0  50
PFEND

; Color table
PFTABLE "Colors" -1  1
"Col 1"     1   0
"Col 2"     2  1a
"Col 3"     3  2e
"Col 4"     4  43
"Col 5"     5  57
"Col 6"     6  6c
"Col 7"     7  80
PFEND

; Gobo table
PFTABLE "Gobos" -1  2  3
"Open"          11  0  0
"Gobo1 1"       12 50  0
"Gobo1 2"       13 80  0
"Gobo1 3"       14 b4  0
"Gobo1 4"       15 ff  0
"Gobo2 1"       17  0  0
"Gobo2 2"       18  0  0
"Gobo2 3"       19  0  0
"Gobo2 4"       20  0  0
"Gobo2 4"       20  0  0
PFEND

MACRO "RESET UNIT" 1  8
"data"           0  80
"delay"          1  50
"data"           0   0
MEND