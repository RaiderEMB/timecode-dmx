    <Macro Name="Lamp Off">
      <Set Channel="Shutter:7" Delay="7000" />
      <Set Channel="Shutter:2:100" Delay="0" />
    </Macro>
    <Macro Name="Stand Alone">
      <Set Channel="Colour1Func:4,Gobo1Func:3,Shutter:5" Delay="0" />
    </Macro>
