      <Function ID="12" Name="Blue 101" Display="'Blue 101'" Dmx="60~65" Colour="0,0,255" />
      <Function ID="13" Name="Blue/Yellow" Display="'Blue/Yellow'" Dmx="66~71" Colour="0,0,255" />
