      <Function ID="6" Name="Orange" Display="'Orange'" Dmx="85~101" Colour="255,128,0" />
      <Function ID="7" Name="Pink" Display="'Pink'" Dmx="102~107" Colour="255,150,150" />
      <Function ID="8" Name="Blue" Display="'Blue'" Dmx="108~127" Colour="0,0,255" />
