    </Attribute>
    <Attribute ID="Colour1" Name="Colour" Description="" Group="C">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed_Func" ID="1,2,3,4,5,6,7,8" If="'Colour1Func'==1" />
      <Condition Name="Scroll_Func" ID="9,10,11,12" If="'Colour1Func'==2" />
