Device ISO_iRock6S

// Shutter
ChanRef Dimmer
    Vismode Strobe
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 4
    Mapping 0 9 0 0
    Mapping 10 120 1 1
    Mapping 121 134 30 300
    Mapping 135 245 10 100
    Mapping 246 255 1 1
End

// Dimmer
ChanRef Dimmer
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 7
    Mapping 0 255 0 255
End

// Pan
ChanRef Pan
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 1
    Mapping 0 255 -180.000000 180.000000
End

// Tilt
ChanRef Tilt
    Vismode Value
    UseBits 1 2 3 4 5 6 7 8
    DmxOffset 2
    Mapping 0 255 -90.000000 90.000000
End

