      <Condition Name="Prism_Func" ID="4,5,6" If="'Effect_Func'==2" />
      <Condition Name="Frost_Func" ID="7,8,9" If="'Effect_Func'==3" />
      <Else ID="1,2,3,4,5,6,7,8,9" />
