    </Attribute>
    <Attribute ID="Gobo1" Name="Gobo" Description="" Group="G">
      <Locate Locate="1" PowerOn="1" />
      <Condition Name="Fixed_Func" ID="1,2,3,4,5,6" If="'Gobo1Func'==1||'Gobo1Func'==2" />
      <Condition Name="Reset_Func" ID="7,8" If="'Gobo1Func'==3" />
