      <Function ID="6" Name="Gobo 5" Display="'Gobo 5'" Dmx="160~254" GoboSpin="0~0" GoboOrientation="0~0" />
      <Function ID="7" Name="Safe" Display="'Safe'" Dmx="0~0" GoboSpin="0~0" GoboOrientation="0~0" />
