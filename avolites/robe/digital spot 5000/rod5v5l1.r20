MACRO "Lamp On"       1  12   8
"data"                0   0   0
"delay"               1  28
"data"                0   0  61
"delay"               1  28
"data"                0  FF  18
MEND

MACRO "Lamp Off"      2  12   8
"data"                0   0   0
"delay"               1   A
"data"                0   0  61
"delay"               1  28
"data"                0   0  18
MEND

MACRO "Shutdown"      3  12   8
"data"                0   0   0
"delay"               1   A
"data"                0   0  90
"delay"               1  28
"data"                0   0  18
MEND

MACRO "Reset"         4  12   8
"data"                0   0   0
"delay"               1   A
"data"                0   0  75
"delay"               1  28
"data"                0  FF  18
MEND

